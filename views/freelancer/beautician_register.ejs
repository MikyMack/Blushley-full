    <!-- Auth Modal -->
    <div class="modal-overlay" id="authModal">
        <div class="auth-modal">
            <div class="modal-header">
                <h2 class="modal-title" id="modalTitle">Freelancer Registration</h2>
                <button class="close-modal" id="closeModal">&times;</button>
            </div>
            <div class="modal-body">
                <div class="auth-tabs">
                    <button class="auth-tab active" data-tab="register">Register</button>
                    <button class="auth-tab" data-tab="login">Login</button>
                </div>
                <!-- Register Form -->
                <form class="auth-form active" id="registerForm" enctype="multipart/form-data" method="POST" action="/freelance/beautician_register">
                    <div class="form-group">
                        <label class="form-label" for="fullName">Full Name</label>
                        <input type="text" class="form-input" id="fullName" name="fullName" required>
                    </div>
                    <div class="form-group">
                        <label class="form-label" for="email">Email Address</label>
                        <input type="email" class="form-input" id="email" name="email" required>
                    </div>
                    <div class="form-group">
                        <label class="form-label" for="phone">Phone Number</label>
                        <input type="tel" class="form-input" id="phone" name="phone" required>
                    </div>
                    <div class="form-group">
                        <label class="form-label" for="bio">Bio / About You</label>
                        <textarea class="form-input" id="bio" name="bio" rows="3"></textarea>
                    </div>
                    <div class="form-group">
                        <label class="form-label">Service Locations</label>
                        <div id="locations-chips-input" class="chips-input-wrapper">
                            <input type="text" id="locationsInput" class="chips-input" placeholder="Add a location and press Enter">
                        </div>
                        <input type="hidden" id="locations" name="locations" required>
                        <small style="font-size: 12px;">e.g., Trivandrum, Kochi</small>
                    </div>
                    <div class="form-group">
                        <label class="form-label">Your Skills</label>
                        <div id="skills-chips-input" class="chips-input-wrapper">
                            <input type="text" id="skillsInput" class="chips-input" placeholder="Add a skill and press Enter">
                        </div>
                        <input type="hidden" id="skills" name="skills" required>
                        <small style="font-size: 12px;">e.g., Hair, Makeup, Nails</small>
                    </div>
                    <div class="form-group">
                        <label class="form-label">Availability</label>
                        <div style="display: flex; gap: 10px; flex-wrap: wrap; margin-bottom: 7px;">
                            <div class="avail-days">
                                <label>Days:</label>
                                <div class="availability-days" style="display:flex; flex-wrap:wrap; gap:4px; margin-top: 2px;">
                                    <% ["Monday","Tuesday","Wednesday","Thursday","Friday","Saturday","Sunday"].forEach(function(day) { %>
                                        <label class="avail-checkbox">
                                        <input type="checkbox" name="availability_days" value="<%= day %>">
                                         <%= day.substr(0,3) %>
                                        </label>
                                    <% }); %>
                                </div>
                            </div>
                            <div class="avail-time">
                                <label>From:</label>
                                <input type="time" class="form-input" id="availStart" style="width:150px;">
                                <label>To:</label>
                                <input type="time" class="form-input" id="availEnd" style="width:150px;">
                            </div>
                        </div>
                        <input type="hidden" id="availability" name="availability">
                        <small style="font-size: 12px;">Select your working days and time slot</small>
                    </div>
                    <div class="form-group">
                        <label class="form-label" for="profileImage">Profile Picture</label>
                        <div class="file-input-wrapper">
                            <input type="file" class="file-input" id="profileImage" name="profileImage" accept="image/*">
                            <label class="file-input-label" for="profileImage">
                                <i class="fas fa-upload"></i> Choose Profile Picture
                            </label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="form-label">Portfolio Images</label>
                        <div class="file-input-wrapper">
                            <input type="file" class="file-input" id="portfolioImages" name="portfolioImages" multiple accept="image/*">
                            <label class="file-input-label" for="portfolioImages">
                                <i class="fas fa-images"></i> Upload Portfolio (max 5)
                            </label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="form-label" for="proofType">Proof Type</label>
                        <select class="form-select" id="proofType" name="proofType" required>
                            <option value="">Select proof type</option>
                            <option value="Aadhaar Card">Aadhaar Card</option>
                            <option value="PAN Card">PAN Card</option>
                            <option value="Driving License">Driving License</option>
                            <option value="Passport">Passport</option>
                            <option value="Voter ID">Voter ID</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label class="form-label" for="proofNumber">Proof Number</label>
                        <input type="text" class="form-input" id="proofNumber" name="proofNumber" required>
                    </div>

                    <button style="display: flex; text-align: center; justify-content: center;" type="submit" class="btn-primary">Register</button>
                    <div class="form-footer">
                        <p style="display: flex; justify-content: center; align-items: center; text-align: center; flex-direction: column;">
                            Already have an account? <button type="button" class="switch-form" data-switch-to="login">Login here</button>
                        </p>
                    </div>
                </form>

                <!-- Login Form -->
                <form class="auth-form" id="loginForm" autocomplete="off" method="post" action="/freelance/send-login-otp">
                    <div class="form-group">
                        <label class="form-label" for="loginEmail">Email Address</label>
                        <input type="email" class="form-input" id="loginEmail" name="email" required autocomplete="off">
                    </div>
                    <button style="display: flex; justify-content: center;" type="submit" class="btn-primary">Send OTP</button>
                    <div style="display: flex; justify-content: center; align-items: center;" class="form-footer">
                        <p>Don't have an account? <button type="button" class="switch-form" data-switch-to="register">Register here</button></p>
                    </div>
                </form>

                <!-- OTP Verification -->
                <div class="otp-section" id="otpSection">
                    <h3>Enter OTP</h3>
                    <p id="otpMessage">We've sent a 6-digit OTP to your email</p>
                    <div class="otp-inputs">
                        <input type="text" class="otp-input" maxlength="1" data-index="0" inputmode="numeric" pattern="\d*">
                        <input type="text" class="otp-input" maxlength="1" data-index="1" inputmode="numeric" pattern="\d*">
                        <input type="text" class="otp-input" maxlength="1" data-index="2" inputmode="numeric" pattern="\d*">
                        <input type="text" class="otp-input" maxlength="1" data-index="3" inputmode="numeric" pattern="\d*">
