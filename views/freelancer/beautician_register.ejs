    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script>
    document.addEventListener('DOMContentLoaded', function() {
        const authModal = document.getElementById('authModal');
        const closeModalBtn = document.getElementById('closeModal');
        const modalTitle = document.getElementById('modalTitle');
        const authTabs = document.querySelectorAll('.auth-tab');
        const authForms = document.querySelectorAll('.auth-form');
        const switchFormBtns = document.querySelectorAll('.switch-form');
        const registerForm = document.getElementById('registerForm');
        const loginForm = document.getElementById('loginForm');
        const otpSection = document.getElementById('otpSection');
        const otpMessage = document.getElementById('otpMessage');
        const successMessage = document.getElementById('successMessage');
        const otpInputs = document.querySelectorAll('.otp-input');
        const verifyOtpBtn = document.getElementById('verifyOtpBtn');
        const resendOtpBtn = document.getElementById('resendOtpBtn');
        const goToLoginBtn = document.getElementById('goToLoginBtn');

        // UTILITY: Chips component for locations & skills
        function chipsInput(wrapperId, inputId, hiddenId) {
            const wrapper = document.getElementById(wrapperId);
            const input = wrapper.querySelector('input.chips-input');
            const hidden = document.getElementById(hiddenId);
            let values = [];

            function render() {
                wrapper.querySelectorAll('.chip').forEach(chip => chip.remove());
                values.forEach((val, i) => {
                    const chip = document.createElement('span');
                    chip.className = 'chip';
                    chip.textContent = val;
                    const x = document.createElement('button');
                    x.className = 'remove-chip';
                    x.type = 'button';
                    x.innerHTML = '&times;';
                    x.addEventListener('click', (e) => {
                        e.preventDefault();
                        values.splice(i, 1);
                        render();
                    });
                    chip.appendChild(x);
                    wrapper.insertBefore(chip, input);
                });
                hidden.value = JSON.stringify(values);
            }

            input.addEventListener('keydown', function(e) {
                if ((e.key === 'Enter' || e.key === ',') && input.value.trim()) {
                    e.preventDefault();
                    let val = input.value.trim();
                    if(!values.includes(val)) values.push(val);
                    input.value = '';
                    render();
                } else if (e.key === 'Backspace' && input.value === '' && values.length) {
                    values.pop();
                    render();
                }
            });

            input.addEventListener('blur', function() {
                let val = input.value.trim();
                if (val && !values.includes(val)) {
                    values.push(val);
                    input.value = '';
                    render();
                }
            });

            registerForm.addEventListener('reset', function() {
                values = [];
                render();
            });

            render();
        }
        chipsInput('locations-chips-input', 'locationsInput', 'locations');
        chipsInput('skills-chips-input', 'skillsInput', 'skills');

        // AVAILABILITY composition UX
        const daysCheckboxes = document.querySelectorAll('.availability-days input[type="checkbox"]');
        const availStartInput = document.getElementById('availStart');
        const availEndInput = document.getElementById('availEnd');
        const availHidden = document.getElementById('availability');
        function collectAvailability() {
            const selectedDays = Array.from(daysCheckboxes).filter(cb => cb.checked).map(cb => cb.value);
            const start = availStartInput.value;
            const end = availEndInput.value;
            if (selectedDays.length && start && end) {
                availHidden.value = JSON.stringify({ days: selectedDays, start, end });
            } else {
                availHidden.value = '';
            }
        }
        daysCheckboxes.forEach(cb => cb.addEventListener('change', collectAvailability));
        availStartInput.addEventListener('change', collectAvailability);
        availEndInput.addEventListener('change', collectAvailability);
        registerForm.addEventListener('reset', function() {
            daysCheckboxes.forEach(cb => cb.checked = false);
            availStartInput.value = '';
            availEndInput.value = '';
            collectAvailability();
        });
        registerForm.addEventListener('submit', function(e) {
            collectAvailability();
            if (!availHidden.value) {
                e.preventDefault();
                Swal.fire({
                    icon: 'warning',
                    title: 'Missing Availability',
                    text: 'Please select your available days and working time.'
                });
                return false;
            }
        });

        // Show/hide forms logic
        function showForm(formName) {
            authTabs.forEach(tab => {
                tab.classList.remove('active');
                if (tab.getAttribute('data-tab') === formName) {
                    tab.classList.add('active');
                }
            });
            authForms.forEach(form => form.classList.remove('active'));
            if (formName === 'register') {
                modalTitle.textContent = 'Freelancer Registration';
                registerForm.classList.add('active');
            } else {
                modalTitle.textContent = 'Welcome Back';
                loginForm.classList.add('active');
            }
            otpSection.classList.remove('active');
            successMessage.classList.remove('active');
        }
        authTabs.forEach(tab => {
            tab.addEventListener('click', function() {
                showForm(this.getAttribute('data-tab'));
            });
        });
        switchFormBtns.forEach(btn => {
            btn.addEventListener('click', function() {
                showForm(this.getAttribute('data-switch-to'));
            });
        });
        closeModalBtn.addEventListener('click', function() {
            authModal.classList.remove('active');
            resetForms();
        });
        authModal.addEventListener('click', function(e) {
            if (e.target === authModal) {
                authModal.classList.remove('active');
                resetForms();
            }
        });

        // Helper: Set loading on button
        function setLoading(btn, isLoading, loadingText = 'Loading...') {
            if (!btn) return;
            if (isLoading) {
                btn.dataset.originalText = btn.textContent;
                btn.disabled = true;
                btn.innerHTML = '<span class="spinner-border spinner-border-sm" style="vertical-align:middle;"></span> ' + loadingText;
            } else {
                if (btn.dataset.originalText) {
                    btn.textContent = btn.dataset.originalText;
                }
                btn.disabled = false;
            }
        }

        // Registration form submit: on success, show OTP modal and a message to verify
        registerForm.addEventListener('submit', async function(e) {
            if (e.defaultPrevented) return;
            e.preventDefault();

            const formData = new FormData(registerForm);

            try {
                const locationsVal = document.getElementById('locations').value;
                const skillsVal = document.getElementById('skills').value;
                const availabilityVal = document.getElementById('availability').value;

                if (!locationsVal || JSON.parse(locationsVal).length === 0) {
                    Swal.fire({
                        icon: 'warning',
                        title: 'Missing Location',
                        text: 'Please enter at least one location.'
                    });
                    return;
                }
                if (!skillsVal || JSON.parse(skillsVal).length === 0) {
                    Swal.fire({
                        icon: 'warning',
                        title: 'Missing Skills',
                        text: 'Please enter at least one skill.'
                    });
                    return;
                }
                formData.set('locations', locationsVal);
                formData.set('skills', JSON.parse(skillsVal).join(','));
                formData.set('skillsArray', skillsVal);
                formData.set('availability', availabilityVal);

                // Add loading to submit button
                const submitBtn = registerForm.querySelector('button[type="submit"], input[type="submit"]');
                setLoading(submitBtn, true, 'Registering...');

                const response = await fetch('/freelance/beautician_register', {
                    method: 'POST',
                    body: formData
                });

                setLoading(submitBtn, false);

                if (response.ok) {
                    registerForm.classList.remove('active');
                    otpSection.classList.add('active');
                    modalTitle.textContent = 'Verify Your Email';

                    const emailVal = registerForm.querySelector('#email').value.trim();
                    if (otpMessage) {
                        otpMessage.textContent = 'Please enter the OTP sent to your email: ' + emailVal;
                        otpMessage.style.display = '';
                    }
                    otpInputs.forEach(input => input.value = '');
                    if(otpInputs[0]) otpInputs[0].focus();

                    Swal.fire({
                        icon: 'success',
                        title: 'Registration Successful!',
                        html: 'Your account has been created successfully.<br>Please verify your email to continue.',
                        timer: 2400,
                        showConfirmButton: false
                    });
                } else {
                    const errJson = await response.json().catch(() => ({}));
                    Swal.fire({
                        icon: 'error',
                        title: 'Registration Failed',
                        text: errJson.message || "Registration failed. Please try again."
                    });
                }
            } catch (err) {
                const submitBtn = registerForm.querySelector('button[type="submit"], input[type="submit"]');
                setLoading(submitBtn, false);
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: "An error occurred. Please try again."
                });
            }
        });

        // Login: send OTP to email using /freelance/send-login-otp (for login only)
        loginForm.addEventListener('submit', async function(e) {
            e.preventDefault();
            const email = loginForm.querySelector('#loginEmail').value.trim();
            if (!email) {
                Swal.fire({
                    icon: 'warning',
                    title: 'Missing Email',
                    text: 'Please enter your email.'
                });
                return;
            }

            const submitBtn = loginForm.querySelector('button[type="submit"], input[type="submit"]');
            setLoading(submitBtn, true, 'Sending OTP...');
            try {
                const response = await fetch('/freelance/send-login-otp', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({ email })
                });
                const result = await response.json();
                setLoading(submitBtn, false);
                if (response.ok && result.success) {
                    loginForm.classList.remove('active');
                    otpSection.classList.add('active');
                    modalTitle.textContent = 'Verify Login OTP';
                    if (otpMessage) {
                        otpMessage.textContent = 'Please enter the OTP sent to your email: ' + email;
                        otpMessage.style.display = '';
                    }
                    otpInputs.forEach(input => input.value = '');
                    if(otpInputs[0]) otpInputs[0].focus();

                    Swal.fire({
                        icon: 'success',
                        title: 'OTP Sent',
                        html: 'An OTP has been sent to your email. Please check your inbox.',
                        timer: 2000,
                        showConfirmButton: false
                    });
                } else {
                    Swal.fire({
                        icon: 'error',
                        title: 'Failed to Send OTP',
                        text: result.message || 'Could not send OTP. Try again.'
                    });
                }
            } catch (err) {
                setLoading(submitBtn, false);
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'An error occurred while sending OTP.'
                });
            }
        });

        // OTP input UX
        otpInputs.forEach((input, index) => {
            input.addEventListener('input', function() {
                if (this.value.match(/\d/) && this.value.length === 1 && index < otpInputs.length - 1) {
                    otpInputs[index + 1].focus();
                }
            });
            input.addEventListener('keydown', function(e) {
                if (e.key === 'Backspace' && this.value.length === 0 && index > 0) {
                    otpInputs[index - 1].focus();
                }
            });
        });

        // OTP verification logic
        verifyOtpBtn.addEventListener('click', async function(e) {
            e.preventDefault();
            let otp = '';
            otpInputs.forEach(input => otp += input.value.trim());
            if (otp.length !== 6) {
                Swal.fire({
                    icon: 'warning',
                    title: 'Invalid OTP',
                    text: 'Please enter complete 6-digit OTP'
                });
                return;
            }

            let email = '';
            let forLogin = false;
            // If login tab or OTP entered after login, use loginForm email and login route
            if (loginForm.classList.contains('active') || modalTitle.textContent.indexOf('Login') !== -1) {
                email = loginForm.querySelector('#loginEmail').value.trim();
                forLogin = true;
            } else {
                email = registerForm.querySelector('#email').value.trim();
                forLogin = false;
            }
            if (!email) {
                Swal.fire({
                    icon: 'warning',
                    title: 'Missing Email',
                    text: 'Please enter email.'
                });
                return;
            }

            setLoading(verifyOtpBtn, true, 'Verifying...');
            try {
                let response, result;
                if (forLogin) {
                    response = await fetch('/freelance/verify-login-otp', {
                        method: 'POST',
                        headers: { 'Content-Type': 'application/json' },
                        body: JSON.stringify({ email, otp })
                    });
                    result = await response.json();
                } else {
                    // Registration verification (generic, still using main OTP route)
                    response = await fetch('/verify-otp', {
                        method: 'POST',
                        headers: { 'Content-Type': 'application/json' },
                        body: JSON.stringify({ email, otp, purpose: 'freelancer_verify' })
                    });
                    result = await response.json();
                }
                setLoading(verifyOtpBtn, false);

                if (response.ok && result.success) {
                    Swal.fire({
                        icon: 'success',
                        title: forLogin ? 'Login successful!' : 'Verification successful!',
                        html: (result.message ? result.message + '<br>' : '') +
                              (result.status === "pending" && result.role === "freelancer"
                                ? `<div style="margin-top:5px">Your freelancer profile is under admin review.</div>`
                                : '') +
                              "Redirecting...",
                        timer: 2300,
                        timerProgressBar: true,
                        showConfirmButton: false
                    }).then(() => {
                        window.location.href = result.redirect || result.redirectTo || (forLogin ? '/freelance/beautician_dashboard' : '/freelance/beautician_register');
                        resetForms();
                    });
                } else {
                    Swal.fire({
                        icon: 'error',
                        title: forLogin ? 'OTP Verification Failed' : 'OTP Verification Failed',
                        text: result.message || "OTP verification failed. Try again."
                    });
                    otpInputs.forEach(input => input.value = '');
                    if (otpInputs[0]) otpInputs[0].focus();
                }
            } catch (err) {
                setLoading(verifyOtpBtn, false);
                Swal.fire({
                    icon: 'error',
                    title: 'Error verifying OTP',
                    text: 'Please try again.'
                });
                otpInputs.forEach(input => input.value = '');
                if (otpInputs[0]) otpInputs[0].focus();
            }
        });

        // Resend OTP logic
        resendOtpBtn.addEventListener('click', async function() {
            otpInputs.forEach(input => { input.value = ''; });
            if(otpInputs[0]) otpInputs[0].focus();

            let email = '';
            let forLogin = false;
            if (loginForm.classList.contains('active') || modalTitle.textContent.indexOf('Login') !== -1) {
                email = loginForm.querySelector('#loginEmail').value.trim();
                forLogin = true;
            } else {
                email = registerForm.querySelector('#email').value.trim();
                forLogin = false;
            }
            if (!email) {
                Swal.fire({
                    icon: 'warning',
                    title: 'Missing Email',
                    text: 'Please enter your email.'
                });
                return;
            }

            setLoading(resendOtpBtn, true, 'Resending...');
            try {
                let response, result;
                if (forLogin) {
                    // Use the send-login-otp endpoint for login
                    response = await fetch('/freelance/send-login-otp', {
                        method: 'POST',
                        headers: { 'Content-Type': 'application/json' },
                        body: JSON.stringify({ email })
                    });
                } else {
                    // Use the registration OTP endpoint for resending registration verification
                    response = await fetch('/auth/send-otp', {
                        method: 'POST',
                        headers: { 'Content-Type': 'application/json' },
                        body: JSON.stringify({ email, purpose: 'freelancer_verify' })
                    });
                }
                result = await response.json();

                setLoading(resendOtpBtn, false);
                if (response.ok && result.success) {
                    Swal.fire({
                        icon: 'success',
                        title: 'OTP Resent',
                        html: 'OTP has been resent to your email.',
                        timer: 1800,
                        showConfirmButton: false
                    });
                    if (otpMessage) {
                        otpMessage.textContent = 'Please enter the OTP sent to your email: ' + email;
                        otpMessage.style.display = '';
                    }
                } else {
                    Swal.fire({
                        icon: 'error',
                        title: 'Failed to Resend OTP',
                        text: result.message || 'Could not resend OTP.'
                    });
                }
            } catch (err) {
                setLoading(resendOtpBtn, false);
                Swal.fire({
                    icon: 'error',
                    title: 'Error resending OTP',
                    text: 'Please try again.'
                });
            }
        });

        goToLoginBtn.addEventListener('click', function() {
            showForm('login');
        });

        function resetForms() {
            registerForm.reset();
            loginForm.reset();
            otpInputs.forEach(input => input.value = '');
            showForm('register');
            if (otpMessage) {
                otpMessage.textContent = '';
                otpMessage.style.display = 'none';
            }
        }
    });
    </script>

