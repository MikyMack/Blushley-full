                                <script>
                                    // Global variables
                                    let currentPage = 1;
                                    let limit = 10;
                                    let searchQuery = '';
                                    let startDate = '';
                                    let endDate = '';
                                    let users = [];
                                    let userToMakeAdmin = null;
                                    let userToBlock = null;

                                    // DOM Content Loaded
                                    document.addEventListener('DOMContentLoaded', function () {
                                        // Initialize
                                        fetchUsers();

                                        // Event listeners
                                        document.getElementById('limit-select').addEventListener('change', function () {
                                            limit = parseInt(this.value);
                                            currentPage = 1;
                                            fetchUsers();
                                        });

                                        document.getElementById('search-btn').addEventListener('click', function () {
                                            searchQuery = document.getElementById('search-input').value;
                                            currentPage = 1;
                                            fetchUsers();
                                        });

                                        document.getElementById('apply-date-filter').addEventListener('click', function () {
                                            startDate = document.getElementById('start-date').value;
                                            endDate = document.getElementById('end-date').value;
                                            currentPage = 1;
                                            fetchUsers();
                                        });

                                        document.getElementById('download-user-list').addEventListener('click', function () {
                                            downloadUserList();
                                        });

                                        document.getElementById('download-user-info').addEventListener('click', function () {
                                            downloadUserInfo();
                                        });

                                        document.getElementById('confirm-make-admin-btn').addEventListener('click', function () {
                                            if (userToMakeAdmin) {
                                                makeUserAdmin(userToMakeAdmin);
                                                userToMakeAdmin = null;
                                            }
                                        });

                                        document.getElementById('confirm-block-user-btn').addEventListener('click', function () {
                                            if (userToBlock) {
                                                blockUser(userToBlock);
                                                userToBlock = null;
                                            }
                                        });
                                    });

                                    // Fetch users from API
                                    async function fetchUsers() {
                                        try {
                                            // In a real implementation, this would be an API call
                                            // For demo purposes, we'll use mock data
                                            users = getMockUsers();
                                            
                                            // Filter by search query if provided
                                            if (searchQuery) {
                                                users = users.filter(user => 
                                                    user.name.toLowerCase().includes(searchQuery.toLowerCase()) ||
                                                    user.email.toLowerCase().includes(searchQuery.toLowerCase()) ||
                                                    user.phone.includes(searchQuery)
                                                );
                                            }
                                            
                                            // Filter by date range if provided
                                            if (startDate && endDate) {
                                                users = users.filter(user => {
                                                    const userDate = new Date(user.joinedDate);
                                                    const start = new Date(startDate);
                                                    const end = new Date(endDate);
                                                    return userDate >= start && userDate <= end;
                                                });
                                            }
                                            
                                            renderUsers(users);
                                            renderPagination(Math.ceil(users.length / limit), currentPage);
                                            document.getElementById('showing-entries').textContent = `Showing ${Math.min(users.length, limit)} of ${users.length} entries`;
                                        } catch (error) {
                                            console.error('Error fetching users:', error);
                                            alert('Failed to load users. Please try again later.');
                                            document.getElementById('users-list').innerHTML = '<div class="error-message">Failed to load users</div>';
                                        }
                                    }

                                    // Render user cards
                                    function renderUsers(users) {
                                        const usersList = document.getElementById('users-list');
                                        usersList.innerHTML = '';

                                        // Calculate pagination
                                        const startIndex = (currentPage - 1) * limit;
                                        const endIndex = startIndex + limit;
                                        const paginatedUsers = users.slice(startIndex, endIndex);

                                        if (paginatedUsers.length === 0) {
                                            usersList.innerHTML = '<div class="no-users">No users found matching your criteria.</div>';
                                            return;
                                        }

                                        paginatedUsers.forEach(user => {
                                            const userCard = document.createElement('div');
                                            userCard.className = `user-card ${user.isPremium ? 'premium' : ''}`;
                                            userCard.id = `user-${user.id}`;

                                            const profileImage = user.image || 'https://i.pravatar.cc/150?img=' + Math.floor(Math.random() * 70);

                                            userCard.innerHTML = `
                                                <div class="user-header">
                                                    <div class="user-info-main">
                                                        <div class="user-avatar">
                                                            <img src="${profileImage}" alt="${user.name}">
                                                        </div>
                                                        <div class="user-details">
                                                            <div class="user-name">
                                                                ${user.name}
                                                                ${user.isVerified ? '<i class="fa fa-check-circle" style="color: #1890ff;"></i>' : ''}
                                                            </div>
                                                            <div class="user-email">${user.email}</div>
                                                            <div class="user-phone">
                                                                <i class="icon-phone"></i> ${user.phone}
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="user-status">
                                                        <span class="role-badge ${user.role}">${user.role}</span>
                                                        <span class="status-badge ${user.isActive ? 'active' : 'inactive'}">
                                                            <i class="fa ${user.isActive ? 'fa-check-circle' : 'fa-times-circle'}"></i>
                                                            ${user.isActive ? 'Active' : 'Blocked'}
                                                        </span>
                                                    </div>
                                                </div>
                                                <div class="user-body">
                                                    <div class="user-metrics">
                                                        <div class="metric-item">
                                                            <div class="metric-value">â‚¹${user.wallet}</div>
                                                            <div class="metric-label">Wallet</div>
                                                        </div>
                                                        <div class="metric-item">
                                                            <div class="metric-value">${user.totalOrders}</div>
                                                            <div class="metric-label">Orders</div>
                                                        </div>
                                                        <div class="metric-item">
                                                            <div class="metric-value">${user.totalBookings}</div>
                                                            <div class="metric-label">Bookings</div>
                                                        </div>
                                                        <div class="metric-item">
                                                            <div class="metric-value">${user.joinedDate.split('-')[0]}</div>
                                                            <div class="metric-label">Joined</div>
                                                        </div>
                                                    </div>
                                                    <div class="user-actions">
                                                        <button class="btn btn-info btn-sm" onclick="showUserDetails('${user.id}')">
                                                            <i class="icon-eye"></i> View Details
                                                        </button>
                                                        ${user.role !== 'admin' ? `
                                                            <button class="btn btn-success btn-sm" onclick="confirmMakeAdmin('${user.id}')">
                                                                <i class="icon-user-plus"></i> Make Admin
                                                            </button>
                                                        ` : ''}
                                                        <button class="btn ${user.isActive ? 'btn-warning' : 'btn-secondary'} btn-sm" onclick="confirmBlockUser('${user.id}')">
                                                            <i class="icon-user-x"></i> ${user.isActive ? 'Block' : 'Unblock'}
                                                        </button>
                                                    </div>
                                                </div>
                                                <div class="user-footer">
                                                    <div class="user-tags">
                                                        ${user.isPremium ? '<span class="role-badge vip">Premium</span>' : ''}
                                                        ${user.isVerified ? '<span class="status-badge verified"><i class="fa fa-check-circle"></i> Verified</span>' : ''}
                                                        <span class="text-tiny">ID: ${user.id}</span>
                                                    </div>
                                                    <div class="user-actions-footer">
                                                        <span class="text-tiny">Last login: ${user.lastLogin}</span>
                                                    </div>
                                                </div>
                                            `;

                                            usersList.appendChild(userCard);
                                        });
                                    }

                                    // Show user details modal
                                    function showUserDetails(userId) {
                                        const user = users.find(u => u.id === userId);
                                        if (!user) {
                                            alert('User not found');
                                            return;
                                        }

                                        // Populate user details
                                        document.getElementById('detail-user-image').src = user.image || 'https://i.pravatar.cc/150?img=' + Math.floor(Math.random() * 70);
                                        document.getElementById('detail-user-name').textContent = user.name;
                                        document.getElementById('detail-user-email').textContent = user.email;
                                        document.getElementById('detail-user-phone').textContent = user.phone;
                                        document.getElementById('detail-user-wallet').textContent = `â‚¹${user.wallet}`;
                                        document.getElementById('detail-user-role').textContent = user.role;
                                        document.getElementById('detail-user-status').textContent = user.isActive ? 'Active' : 'Blocked';
                                        document.getElementById('detail-user-joined').textContent = user.joinedDate;
                                        document.getElementById('detail-user-last-login').textContent = user.lastLogin;
                                        document.getElementById('detail-user-orders').textContent = user.totalOrders;

                                        // Set badges
                                        const badgesContainer = document.getElementById('detail-user-badges');
                                        badgesContainer.innerHTML = '';
                                        badgesContainer.innerHTML += `<span class="role-badge ${user.role}">${user.role}</span>`;
                                        badgesContainer.innerHTML += `<span class="status-badge ${user.isActive ? 'active' : 'inactive'}">${user.isActive ? 'Active' : 'Blocked'}</span>`;
                                        if (user.isVerified) {
                                            badgesContainer.innerHTML += '<span class="status-badge verified">Verified</span>';
                                        }
                                        if (user.isPremium) {
                                            badgesContainer.innerHTML += '<span class="role-badge vip">Premium</span>';
                                        }

                                        // Show modal
                                        document.getElementById('user-details-modal').style.display = 'block';
                                    }

                                    // Confirm make admin
                                    function confirmMakeAdmin(userId) {
                                        const user = users.find(u => u.id === userId);
                                        if (!user) {
                                            alert('User not found');
                                            return;
                                        }

                                        userToMakeAdmin = userId;
                                        document.getElementById('make-admin-message').textContent = `Are you sure you want to make ${user.name} an admin?`;
                                        document.getElementById('make-admin-modal').style.display = 'block';
                                    }

                                    // Make user admin
                                    function makeUserAdmin(userId) {
                                        // In a real implementation, this would be an API call
                                        const userIndex = users.findIndex(u => u.id === userId);
                                        if (userIndex !== -1) {
                                            users[userIndex].role = 'admin';
                                        }

                                        alert('User has been made admin successfully!');
                                        closeModal('make-admin-modal');
                                        fetchUsers();
                                    }

                                    // Confirm block user
                                    function confirmBlockUser(userId) {
                                        const user = users.find(u => u.id === userId);
                                        if (!user) {
                                            alert('User not found');
                                            return;
                                        }

                                        userToBlock = userId;
                                        const action = user.isActive ? 'block' : 'unblock';
                                        document.getElementById('block-user-message').textContent = `Are you sure you want to ${action} ${user.name}?`;
                                        document.getElementById('confirm-block-user-btn').textContent = action === 'block' ? 'Block User' : 'Unblock User';
                                        document.getElementById('block-user-modal').style.display = 'block';
                                    }

                                    // Block/unblock user
                                    function blockUser(userId) {
                                        // In a real implementation, this would be an API call
                                        const userIndex = users.findIndex(u => u.id === userId);
                                        if (userIndex !== -1) {
                                            users[userIndex].isActive = !users[userIndex].isActive;
                                        }

                                        const action = users[userIndex].isActive ? 'unblocked' : 'blocked';
                                        alert(`User has been ${action} successfully!`);
                                        closeModal('block-user-modal');
                                        fetchUsers();
                                    }

                                    // Download user list
                                    function downloadUserList() {
                                        // In a real implementation, this would generate a CSV or PDF
                                        alert('User list download functionality would be implemented here. This would typically generate a CSV or PDF file with all user data.');
                                    }

                                    // Download user info
                                    function downloadUserInfo() {
                                        // In a real implementation, this would generate a PDF with user details
                                        alert('User info download functionality would be implemented here. This would typically generate a PDF with the user details shown in the modal.');
                                    }

                                    // Close modal
                                    function closeModal(modalId) {
                                        document.getElementById(modalId).style.display = 'none';
                                    }

                                    // Pagination functions
                                    function renderPagination(totalPages, currentPage) {
                                        const paginationContainer = document.getElementById('pagination');
                                        paginationContainer.innerHTML = '';

                                        // Always show first page button
                                        addPageButton(paginationContainer, 1, currentPage);

                                        // Show previous ellipsis if needed
                                        if (currentPage > 3) {
                                            const ellipsis = document.createElement('li');
                                            ellipsis.innerHTML = '<span>...</span>';
                                            paginationContainer.appendChild(ellipsis);
                                        }

                                        // Show pages around current page
                                        const startPage = Math.max(2, currentPage - 1);
                                        const endPage = Math.min(totalPages - 1, currentPage + 1);

                                        for (let i = startPage; i <= endPage; i++) {
                                            addPageButton(paginationContainer, i, currentPage);
                                        }

                                        // Show next ellipsis if needed
                                        if (currentPage < totalPages - 2) {
                                            const ellipsis = document.createElement('li');
                                            ellipsis.innerHTML = '<span>...</span>';
                                            paginationContainer.appendChild(ellipsis);
                                        }

                                        // Always show last page button if there are multiple pages
                                        if (totalPages > 1) {
                                            addPageButton(paginationContainer, totalPages, currentPage);
                                        }
                                    }

                                    function addPageButton(container, pageNumber, currentPage) {
                                        const pageItem = document.createElement('li');
                                        if (pageNumber === currentPage) {
                                            pageItem.className = 'active';
                                        }

                                        const pageLink = document.createElement('a');
                                        pageLink.href = '#';
                                        pageLink.textContent = pageNumber;
                                        pageLink.onclick = (e) => {
                                            e.preventDefault();
                                            currentPage = pageNumber;
                                            fetchUsers();
                                        };

                                        pageItem.appendChild(pageLink);
                                        container.appendChild(pageItem);
                                    }

