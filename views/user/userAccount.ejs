                            <!--notifications  ICON with Loyalty points -->
                            <% if (user && user.role === "user") { %>
                                <li class="nav-loyalty">
                                    <a href="#" class="nav-icon-item" id="loyaltyToggle">
                                        <i class="fa-regular fa-bell"></i>
                                        <% 
                                          // Show count of unread notifications
                                          var unread = Array.isArray(user.notifications) ? user.notifications.filter(n => !n.read).length : 0;
                                        %>
                                        <% if (unread > 0) { %>
                                          <span class="loyalty-badge"><%= unread %></span>
                                        <% } %>
                                    </a>
  
                                    <!-- Loyalty and Notification Panel -->
                                    <div class="loyalty-panel" id="loyaltyPanel">
                                        <div class="loyalty-header">
                                            <h5 class="mb-0">Notifications</h5>
                                            <% if ((user.notifications && user.notifications.length) > 0) { %>
                                              <button class="mark-all-read" id="markAllRead" title="Mark all as read" type="button">Mark All</button>
                                            <% } %>
                                            <button type="button" class="btn-close" id="closeLoyalty"
                                                aria-label="Close"></button>
                                        </div>
  
                                        <div class="loyalty-points-display">
                                            <% 
                                              var points = (user.wallet && typeof user.wallet.loyaltyPoints !== 'undefined') ? user.wallet.loyaltyPoints : 0;
                                            %>
                                            <div class="points-number"><%= points %></div>
                                            <div class="points-label">Loyalty Points</div>
                                            <% if (!points || points === 0) { %>
                                              <div class="loyalty-zero-msg text-muted mt-2">
                                                Start ordering and book to get points!
                                              </div>
                                            <% } %>
                                        </div>
  
                                        <div class="loyalty-activity">
                                            <h6 class="mb-3">Recent Notifications</h6>
                                            <% if (user.notifications && user.notifications.length > 0) { %>
                                              <ul class="notification-list list-unstyled mb-0">
                                                <% user.notifications.slice().reverse().forEach(function(notif) { %>
                                                  <li class="notification-item d-flex align-items-start <% if(!notif.read){ %> notif-unread <% } %>" data-id="<%= notif._id %>">
                                                      <div class="notification-message flex-grow-1">
                                                          <%= notif.message %>
                                                          <div class="notif-meta">
                                                              <small class="text-muted">
                                                                <%= notif.createdAt ? (new Date(notif.createdAt).toLocaleString("en-IN", {
                                                                     hour: "2-digit", minute: "2-digit", day: "2-digit", month: "short", year: "numeric"
                                                                  })) : "" %>
                                                              </small>
                                                          </div>
                                                      </div>
                                                      <div class="notification-actions ms-2">
                                                          <% if (!notif.read) { %>
                                                            <button class="btn btn-link btn-sm p-0 mark-read" title="Mark as read"><i class="fa fa-check"></i></button>
                                                          <% } %>
                                                      </div>
                                                  </li>
                                                <% }); %>
                                              </ul>
                                            <% } else { %>
                                              <div class="text-center text-muted">No notifications yet.</div>
                                            <% } %>
                                        </div>
                                    </div>
                                    <script>
                                      document.addEventListener('DOMContentLoaded', function() {
                                        const panel = document.getElementById('loyaltyPanel');
                                        const markAllBtn = document.getElementById('markAllRead');

                                        function updateBadgeFromNotifications(notifs) {
                                          const badge = document.querySelector('.loyalty-badge');
                                          if (!notifs || notifs.length === 0) {
                                            if (badge) badge.remove();
                                            return;
                                          }
                                          const unreadCount = notifs.reduce((c, n) => c + (!n.read ? 1 : 0), 0);
                                          if (!badge && unreadCount > 0) {
                                            const newBadge = document.createElement('span');
                                            newBadge.className = 'loyalty-badge';
                                            newBadge.textContent = unreadCount;
                                            const toggle = document.getElementById('loyaltyToggle');
                                            if (toggle && toggle.parentNode) toggle.parentNode.appendChild(newBadge);
                                          } else if (badge) {
                                            if (unreadCount <= 0) badge.remove();
                                            else badge.textContent = unreadCount;
                                          }
                                        }
                 
                                        async function removeNotificationRequest(notifId) {
                                          try {
                                            const res = await fetch(`/notifications/${encodeURIComponent(notifId)}`, { method: 'DELETE', headers: {'Accept':'application/json'} });
                                            const data = await res.json();
                                            if (!res.ok || !data.success) throw new Error(data.message || 'Failed');
                                            return data.notifications || [];
                                          } catch (err) {
                                            console.error('Remove notification failed', err);
                                            throw err;
                                          }
                                        }
           
                                        async function removeAllNotificationsRequest() {
                                          try {
                                            const res = await fetch('/notifications', { method: 'DELETE', headers: {'Accept':'application/json'} });
                                            const data = await res.json();
                                            if (!res.ok || !data.success) throw new Error(data.message || 'Failed');
                                            return data.notifications || [];
                                          } catch (err) {
                                            console.error('Remove all notifications failed', err);
                                            throw err;
                                          }
                                        }

                                        // Fix: Attach Mark All Read handler directly for button
                                        if (markAllBtn) {
                                          markAllBtn.addEventListener('click', async function(e) {
                                            e.preventDefault();
                                            try {
                                              const notifs = await removeAllNotificationsRequest();
                                              // remove all list items
                                              const list = panel.querySelector('.notification-list');
                                              if (list) list.innerHTML = '<div class="text-center text-muted">No notifications yet.</div>';
                                              updateBadgeFromNotifications(notifs);
                                            } catch (err) {
                                              alert('Could not remove all notifications. Try again.');
                                            }
                                          });
                                        }
                    
                                        panel.addEventListener('click', async function(e) {
                                          const target = e.target;
                          
                                          const notifItem = target.closest('.notification-item');
                                          if (!notifItem) {
                                            // Do not handle markAllRead inside panel click anymore!
                                            // (handled above directly on button)
                                            return;
                                          }
                            
                                          if (target.closest('.mark-read')) {
                                            e.preventDefault();
                                            const notifId = notifItem.getAttribute('data-id');
                                            if (!notifId) return;
                                            const backup = notifItem.cloneNode(true);
                                            notifItem.remove();
                                            try {
                                              const notifs = await removeNotificationRequest(notifId);
                                              updateBadgeFromNotifications(notifs);
                                            } catch (err) {
                                              // restore on error
                                              const list = panel.querySelector('.notification-list');
                                              if (list && backup) list.prepend(backup);
                                              alert('Failed to mark notification as read. Try again.');
                                            }
                                            return;
                                          }
                                      
                                          if (target.closest('.remove-notif')) {
                                            e.preventDefault();
                                            const notifId = notifItem.getAttribute('data-id');
                                            if (!notifId) return;
                                            // remove confirmation optional
                                            if (!confirm('Delete this notification?')) return;
                                            const backup = notifItem.cloneNode(true);
                                            notifItem.remove();
                                            try {
                                              const notifs = await removeNotificationRequest(notifId);
                                              updateBadgeFromNotifications(notifs);
                                            } catch (err) {
                                              const list = panel.querySelector('.notification-list');
                                              if (list && backup) list.prepend(backup);
                                              alert('Failed to delete notification. Try again.');
                                            }
                                            return;
                                          }
                                      
                                        }); // panel click
                                      
                                        // close button
                                        const closeBtn = document.getElementById('closeLoyalty');
                                        if (closeBtn) closeBtn.addEventListener('click', () => panel.classList.remove('active'));
                                      
                                        // set initial badge state (from server-rendered notifications in page)
                                        try {
                                          const serverNotifs = <%- JSON.stringify(user.notifications || []) %>;
                                          updateBadgeFromNotifications(serverNotifs);
                                        } catch (e) {
                                          // if embedding JSON fails, ignore
                                        }
                                      });
                                      </script>
                                      
                          
                                </li>
                              <% } %>

