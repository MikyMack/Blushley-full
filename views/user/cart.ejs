        <script>
            // Check if user is logged in (client-side check)
            var isUserLoggedIn = <%= (user && user.role === "user") ? 'true' : 'false' %>;
            
            console.log('=== CART DEBUG START ===');
            console.log('Is user logged in (EJS):', isUserLoggedIn);
            console.log('User object from server:', <%- JSON.stringify(user || null) %>);
            console.log('Current URL:', window.location.href);
            console.log('=== CART DEBUG END ===');
        
            // Fetch cart data from API and render it
            async function fetchCart() {
                console.log('fetchCart() called');
                
                try {
                    // Only fetch if user is logged in
                    if (!isUserLoggedIn) {
                        console.log('User not logged in, showing empty cart');
                        renderCartItems([]);
                        return;
                    }
        
                    console.log('Making fetch request to /listCart');
                    const res = await fetch('/listCart', {
                        credentials: 'same-origin'
                    });
                    
                    console.log('Response status:', res.status, 'OK:', res.ok);
                    
                    if (!res.ok) {
                        throw new Error(`Failed to fetch cart: ${res.status} ${res.statusText}`);
                    }
                    
                    const data = await res.json();
                    console.log('Cart API Response data:', data);
        
                    let items = [];
                    if (data && data.success && data.cart && Array.isArray(data.cart.items)) {
                        items = data.cart.items;
                        console.log(`Found ${items.length} cart items`);
                    } else {
                        console.log('No valid cart items found in response');
                    }
                    
                    renderCartItems(items);
                } catch (err) {
                    console.error('Error fetching cart:', err);
                    console.error('Error stack:', err.stack);
                    renderCartItems([]);
                }
            }
        
            function formatCurrency(val) {
                const num = Number(val) || 0;
                return '₹' + num.toLocaleString('en-IN', {
                    minimumFractionDigits: 2,
                    maximumFractionDigits: 2
                });
            }
        
            function escape(str) {
                if (!str) return '';
                try {
                    const div = document.createElement('div');
                    div.textContent = str;
                    return div.innerHTML;
                } catch (e) {
                    console.error('Error escaping string:', e);
                    return String(str).replace(/[&<>"']/g, function(m) {
                        return {
                            '&': '&amp;',
                            '<': '&lt;',
                            '>': '&gt;',
                            '"': '&quot;',
                            "'": '&#39;'
                        }[m];
                    });
                }
            }
        
            function renderCartItems(items) {
                console.log('renderCartItems() called with items:', items);
                console.log('Document readyState:', document.readyState);
                
                // Check if DOM is ready
                if (document.readyState === 'loading') {
                    console.log('DOM still loading, waiting...');
                    document.addEventListener('DOMContentLoaded', function() {
                        console.log('DOMContentLoaded fired, calling renderCartItems again');
                        renderCartItems(items);
                    });
                    return;
                }
                
        
                var tbody = document.getElementById('cart-table-rows');
                var clearBtn = document.getElementById('cart-clear-btn');
                var checkoutBtn = document.getElementById('checkout-btn');
                
                console.log('Elements found:');
                console.log('- tbody:', tbody ? 'Found' : 'NOT FOUND');
                console.log('- clearBtn:', clearBtn ? 'Found' : 'NOT FOUND');
                console.log('- checkoutBtn:', checkoutBtn ? 'Found' : 'NOT FOUND');
                
                if (!tbody) {
                    console.error('CRITICAL: Cart table body not found!');
                    console.error('Searching for alternative elements...');
                    
                    // Try to find any cart-related elements
                    const allElements = document.querySelectorAll('*');
                    console.log('All elements with IDs:');
                    allElements.forEach(el => {
                        if (el.id) console.log(`ID: ${el.id}, Tag: ${el.tagName}`);
                    });
                    
                    return;
                }
        
                let subtotal = 0;
                tbody.innerHTML = '';
        
                if (!items || !items.length) {
                    console.log('Cart is empty, showing empty message');
                    tbody.innerHTML = '<tr><td colspan="5" class="text-center py-4">Your cart is empty.</td></tr>';
                    if (clearBtn) clearBtn.style.display = "none";
                    if (checkoutBtn) {
                        checkoutBtn.style.pointerEvents = "none";
                        checkoutBtn.style.opacity = "0.5";
                        checkoutBtn.classList.add('disabled');
                    }
                } else {
                    console.log(`Rendering ${items.length} cart items`);
                    
                    if (clearBtn) clearBtn.style.display = "";
                    if (checkoutBtn) {
                        checkoutBtn.style.pointerEvents = "auto";
                        checkoutBtn.style.opacity = "1";
                        checkoutBtn.classList.remove('disabled');
                    }
                    
                    items.forEach(function(item, index) {
                        console.log(`Processing cart item ${index + 1}/${items.length}:`, item);
                        
                        let product = item.productId || {};
                        
                        // Get product image - check for images array
                        let pImg = '/images/placeholder.png';
                        if (product.images && product.images.length && product.images[0]) {
                            pImg = product.images[0];
                        }
                        
                        let pName = product.title ? product.title : 'Product';
                        
                        // Use correct product detail URL
                        let productSlug = product.slug ? product.slug : product._id;
                        let productDetailLink = productSlug ? ('/productDetails/' + productSlug) : '#';
                        
                        // Calculate item total
                        let itemPrice = Number(item.price) || 0;
                        let itemQuantity = Number(item.quantity) || 1;
                        let itemTotal = itemPrice * itemQuantity;
                        subtotal += itemTotal;
                        
                        console.log(`Item ${index + 1}: ${pName}, Price: ${itemPrice}, Qty: ${itemQuantity}, Total: ${itemTotal}`);
                        
                        // Create row HTML - SIMPLIFIED VERSION FOR TESTING
                        const rowHTML = `
                        <tr>
                            <td>
                                <div style="display: flex; align-items: center;">
                                    <img src="${pImg}" alt="${pName}" style="width: 60px; height: 60px; object-fit: cover; margin-right: 10px; border: 1px solid #ddd;">
                                    <div>
                                        <strong>${pName}</strong>
                                        ${(item.variantName || item.optionValue) ? 
                                            `<div><small>${item.variantName || ''}: ${item.optionValue || ''}</small></div>` : 
                                            ""}
                                    </div>
                                </div>
                            </td>
                            <td class="text-center">${formatCurrency(itemPrice)}</td>
                            <td class="text-center">
                                <div style="display: inline-flex; align-items: center;">
                                    <button type="button" class="btn btn-sm btn-outline-secondary" onclick="updateQuantity('${item._id}', -1)">-</button>
                                    <input type="text" value="${itemQuantity}" style="width: 40px; text-align: center; margin: 0 5px;" readonly>
                                    <button type="button" class="btn btn-sm btn-outline-secondary" onclick="updateQuantity('${item._id}', 1)">+</button>
                                </div>
                            </td>
                            <td class="text-center">${formatCurrency(itemTotal)}</td>
                            <td class="text-center">
                                <button type="button" class="btn btn-sm btn-danger" onclick="removeCartItem('${item._id}')">X</button>
                            </td>
                        </tr>`;
                        
                        console.log(`Adding row ${index + 1} to tbody`);
                        tbody.innerHTML += rowHTML;
                    });
                    
                    console.log(`Total subtotal: ${subtotal}`);
                }
        
                // Calculate totals
                let discount = 0;
                let shipping = 0;
                let total = subtotal + shipping - discount;
        
                console.log('Updating total displays:');
                console.log('- Subtotal:', subtotal);
                console.log('- Total:', total);
        
                // Update totals display
                var elSubtotal = document.getElementById('cart-subtotal');
                var elDiscount = document.getElementById('cart-discount');
                var elTotal = document.getElementById('cart-total');
                
                console.log('Total elements found:');
                console.log('- cart-subtotal:', elSubtotal ? 'Found' : 'NOT FOUND');
                console.log('- cart-discount:', elDiscount ? 'Found' : 'NOT FOUND');
                console.log('- cart-total:', elTotal ? 'Found' : 'NOT FOUND');
                
                if (elSubtotal) {
                    elSubtotal.textContent = formatCurrency(subtotal);
                    console.log('Updated cart-subtotal to:', formatCurrency(subtotal));
                }
                if (elDiscount) {
                    elDiscount.textContent = '-' + formatCurrency(discount);
                }
                if (elTotal) {
                    elTotal.textContent = formatCurrency(total);
                    console.log('Updated cart-total to:', formatCurrency(total));
                }
            }
        
            // Quantity Update
            function updateQuantity(itemId, delta) {
                console.log('updateQuantity called for item:', itemId, 'delta:', delta);
                
                if (!isUserLoggedIn) {
                    window.location.href = '/userLogin?redirect=' + encodeURIComponent(window.location.pathname);
                    return;
                }
                
                var qtyInput = document.getElementById("qty-" + itemId);
                if (!qtyInput) {
                    console.error('Quantity input not found for item:', itemId);
                    return;
                }
                
                var current = parseInt(qtyInput.value) || 1;
                var updated = Math.max(current + delta, 1);
                
                console.log('Current:', current, 'Updated:', updated);
                
                if (updated !== current) {
                    fetch('/updateCart', {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json',
                        },
                        credentials: 'same-origin',
                        body: JSON.stringify({ 
                            itemId: itemId, 
                            quantity: updated 
                        })
                    })
                    .then(res => {
                        if (!res.ok) throw new Error('Network response was not ok');
                        return res.json();
                    })
                    .then(data => {
                        console.log('Update response:', data);
                        if (data.success) {
                            fetchCart();
                        } else {
                            alert(data.message || 'Failed to update quantity');
                        }
                    })
                    .catch(error => {
                        console.error('Error updating quantity:', error);
                        alert('Failed to update quantity. Please try again.');
                    });
                }
            }
        
            // Remove Cart Item
            function removeCartItem(itemId) {
                console.log('removeCartItem called for item:', itemId);
                
                if (!isUserLoggedIn) {
                    window.location.href = '/userLogin?redirect=' + encodeURIComponent(window.location.pathname);
                    return;
                }
                
                if (confirm("Remove this item from cart?")) {
                    fetch(`/removeCart/${itemId}`, {
                        method: 'DELETE',
                        headers: {
                            'Content-Type': 'application/json'
                        },
                        credentials: 'same-origin'
                    })
                    .then(res => {
                        if (!res.ok) throw new Error('Network response was not ok');
                        return res.json();
                    })
                    .then(data => {
                        console.log('Remove response:', data);
                        if (data.success) {
                            fetchCart();
                        } else {
                            alert(data.message || 'Failed to remove item');
                        }
                    })
                    .catch(error => {
                        console.error('Error removing item:', error);
                        alert('Failed to remove item. Please try again.');
                    });
                }
            }
        
            // Clear Cart
            function clearCart() {
                console.log('clearCart called');
                
                if (!isUserLoggedIn) {
                    window.location.href = '/userLogin?redirect=' + encodeURIComponent(window.location.pathname);
                    return;
                }
                
                if (confirm("Clear all items from cart?")) {
                    fetch('/clearCart', {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json'
                        },
                        credentials: 'same-origin'
                    })
                    .then(res => {
                        if (!res.ok) throw new Error('Network response was not ok');
                        return res.json();
                    })
                    .then(data => {
                        console.log('Clear cart response:', data);
                        if (data.success) {
                            fetchCart();
                        } else {
                            alert(data.message || 'Failed to clear cart');
                        }
                    })
                    .catch(error => {
                        console.error('Error clearing cart:', error);
                        alert('Failed to clear cart. Please try again.');
                    });
                }
            }
        
            // On page load
            document.addEventListener('DOMContentLoaded', function() {
                console.log('=== DOMContentLoaded FIRED ===');
                console.log('isUserLoggedIn:', isUserLoggedIn);
                
                // Check if cart elements exist on this page
                const cartTable = document.getElementById('cart-table-rows');
                console.log('Cart table exists:', !!cartTable);
                
                if (cartTable) {
                    if (isUserLoggedIn) {
                        console.log('Fetching cart...');
                        fetchCart();
                    } else {
                        console.log('User not logged in, showing empty cart');
                        renderCartItems([]);
                    }
                } else {
                    console.log('Cart table not found on this page');
                }
            });
        
            // Also trigger fetch if page is already loaded
            if (document.readyState === 'complete' || document.readyState === 'interactive') {
                console.log('Document already loaded, triggering fetch');
                setTimeout(function() {
                    const cartTable = document.getElementById('cart-table-rows');
                    if (cartTable && isUserLoggedIn) {
                        fetchCart();
                    }
                }, 100);
            }
        
            // Make functions globally available
            window.fetchCart = fetchCart;
            window.updateQuantity = updateQuantity;
            window.removeCartItem = removeCartItem;
            window.clearCart = clearCart;
            
            console.log('Cart functions initialized');
        </script>
