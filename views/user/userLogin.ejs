    <style>
        /* Login Type Selector Styles - Added to match existing design */
        .login-type-selector {
            display: flex;
            margin-bottom: 25px;
            border-radius: 8px;
            overflow: hidden;
            border: 1px solid #e0e0e0;
            background: white;
        }

        .login-type-btn {
            flex: 1;
            padding: 12px 16px;
            background: white;
            border: none;
            font-weight: 600;
            cursor: pointer;
            transition: all 0.3s;
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 8px;
            font-size: 14px;
            color: #333;
        }

        .login-type-btn.active {
            background: var(--primary-color, #4a0514);
            color: white;
            border-radius: 10px;
        }

        .login-type-btn:not(.active):hover {
            background: #f5f5f5;
        }

        .login-form {
            display: none;
        }

        .login-form.active {
            display: block;
        }

        /* Ensure existing design remains unchanged */
        .form-login fieldset {
            margin-bottom: 20px;
        }

        .form-login input,
        .form-login select {
            width: 100%;
            padding: 14px 16px;
            border: 1px solid #e0e0e0;
            border-radius: 8px;
            font-size: 16px;
            transition: all 0.3s;
        }

        .form-login input:focus,
        .form-login select:focus {
            border-color: var(--primary-color, #3f0713);
            box-shadow: 0 0 0 3px rgba(85, 5, 22, 0.2);
            outline: none;
        }

        /* Seller Register Styles */
        .seller-register-panel {
            display: none;
            background: #fff;
            border: 1px solid #e0e0e0;
            border-radius: 12px;
            padding: 16px;
            margin-top: 16px;
        }

        .seller-register-panel.active {
            display: block;
        }

        .seller-register-scroll {
            max-height: 460px;
            /* defined height per requirement */
            overflow-y: auto;
            /* vertical overflow */
            padding-right: 6px;
            /* space for scrollbar */
        }

        .seller-grid {
            display: grid;
            grid-template-columns: 1fr 1fr;
            gap: 12px;
        }

        @media (max-width: 767px) {
            .seller-grid {
                grid-template-columns: 1fr;
            }
        }

        .seller-chip-wrap {
            display: flex;
            flex-wrap: wrap;
            gap: 8px;
            margin-top: 8px;
        }

        .seller-chip {
            display: inline-flex;
            align-items: center;
            gap: 6px;
            background: #f5f5f5;
            border: 1px solid #e0e0e0;
            border-radius: 999px;
            padding: 6px 10px;
            font-size: 13px;
        }

        .seller-chip .remove-chip {
            cursor: pointer;
            color: #999;
            font-weight: 700;
        }

        .seller-section-title {
            display: flex;
            align-items: center;
            justify-content: space-between;
            margin-bottom: 12px;
        }

        .seller-inline {
            display: flex;
            gap: 8px;
        }

        .seller-inline>* {
            flex: 1;
        }

        .btn-link-like {
            background: transparent;
            border: none;
            color: var(--primary-color, #560819);
            font-weight: 600;
            cursor: pointer;
            padding: 0;
        }

        /* Seller Register Inputs & Dropdowns - attractive styles */
        .seller-register-panel input[type="text"],
        .seller-register-panel input[type="email"],
        .seller-register-panel input[type="tel"],
        .seller-register-panel input[type="file"],
        .seller-register-panel select,
        .seller-register-panel textarea {
            width: 100%;
            padding: 14px 16px;
            border: 1px solid #e0e0e0;
            border-radius: 10px;
            font-size: 15px;
            background-color: #fff;
            transition: border-color 0.2s ease, box-shadow 0.2s ease, background-color 0.2s ease;
        }

        .seller-register-panel textarea {
            min-height: 110px;
            resize: vertical;
        }

        .seller-register-panel input::placeholder,
        .seller-register-panel textarea::placeholder {
            color: #9aa0a6;
        }

        .seller-register-panel input:focus,
        .seller-register-panel select:focus,
        .seller-register-panel textarea:focus {
            border-color: var(--primary-color, #540e1d);
            box-shadow: 0 6px 16px rgba(255, 107, 139, 0.12);
            outline: none;
            background-color: #fff;
        }

        /* Custom select arrow */
        .seller-register-panel select {
            -webkit-appearance: none;
            -moz-appearance: none;
            appearance: none;
            background-image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' width='20' height='20' viewBox='0 0 24 24' fill='none' stroke='%238B0000' stroke-width='2' stroke-linecap='round' stroke-linejoin='round'><polyline points='6 9 12 15 18 9'/></svg>");
            background-repeat: no-repeat;
            background-position: right 14px center;
            background-size: 18px 18px;
        }

        /* Improve file input appearance (basic) */
        .seller-register-panel input[type="file"] {
            padding: 10px 12px;
        }

        .login-type-selector {
            margin-top: 1rem;
        }

        /* Section spacing */
        #seller-register-form fieldset {
            margin: 0;
        }

