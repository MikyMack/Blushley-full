                            document.getElementById('reseller-login-form').style.display = 'block';
                        };

                        // Reseller Register Form Submit with SweetAlert, validation, AJAX
                        document.getElementById('reseller-register-form').addEventListener('submit', async function(e) {
                            e.preventDefault();
                            const form = e.target;
                            const fd = new FormData(form);

                            // Only validate fields that are in the schema/registration controller.
                            const requiredFields = [
                                {name: "companyName", label: "Company Name"},
                                {name: "contactPerson", label: "Contact Person's Name"},
                                {name: "email", label: "Email Address"},
                                {name: "phone", label: "Mobile Number"},
                                {name: "address", label: "Address"},
                                {name: "city", label: "City"},
                                {name: "state", label: "State"},
                                {name: "pincode", label: "Pincode"}
                            ];
                            for (const field of requiredFields) {
                                if (!form[field.name].value.trim()) {
                                    Swal.fire({
                                        icon: 'warning',
                                        title: 'Missing Information',
                                        text: `Please enter ${field.label}.`,
                                        confirmButtonColor: '#b50957'
                                    });
                                    form[field.name].focus(); return;
                                }
                            }
                            // Validate phone
                            if (!/^\d{10}$/.test(form.phone.value.trim())) {
                                Swal.fire({
                                    icon: 'warning',
                                    title: 'Invalid Mobile',
                                    text: 'Enter a valid 10-digit mobile number.',
                                    confirmButtonColor: '#b50957'
                                });
                                form.phone.focus(); return;
                            }
                            // Validate pincode
                            if (!/^\d{6}$/.test(form.pincode.value.trim())) {
                                Swal.fire({
                                    icon: 'warning',
                                    title: 'Invalid Pincode',
                                    text: 'Enter a valid 6-digit pincode.',
                                    confirmButtonColor: '#b50957'
                                });
                                form.pincode.focus(); return;
                            }
                            Swal.fire({title: "Registering...", allowOutsideClick: false, didOpen: () => Swal.showLoading()});
                            try {
                                const res = await fetch('/reseller/register', {
                                    method: "POST",
                                    body: fd,
                                });
                                const data = await res.json();
                                Swal.close();
                                if (res.ok && data.success) {
                                    Swal.fire({
                                        icon: "success",
                                        title: "Registration successful!",
                                        text: "Awaiting admin approval. You can login after approval.",
                                        confirmButtonColor: '#b50957'
                                    }).then(()=> {
                                        document.getElementById('reseller-register-panel').style.display = 'none';
                                        document.getElementById('reseller-login-form').style.display = 'block';
                                    });
                                } else {
                                    Swal.fire({
                                        icon: "error",
                                        title: "Registration failed",
                                        text: (data && data.error) ? data.error : "Something went wrong. Please try again.",
                                        confirmButtonColor: '#b50957'
                                    });
                                }
                            } catch (err) {
                                Swal.close();
                                Swal.fire({
                                    icon: "error",
                                    title: "Network error",
                                    text: "Unable to register. Please check your internet connection.",
                                    confirmButtonColor: '#b50957'
                                });
                            }
                        });

                        // ---------- Reseller Email OTP Login Logic -----------
                        // Elements
                        const resellerLoginForm = document.getElementById('reseller-login-form');
                        const resellerLoginEmail = document.getElementById('reseller-login-email');
                        const resellerSendOtpBtn = document.getElementById('reseller-send-otp-btn');
                        const resellerOtpSection = document.getElementById('reseller-otp-section');
                        const resellerOtpInputs = resellerOtpSection ? resellerOtpSection.querySelectorAll('.reseller-otp-input') : [];
                        const resellerVerifyOtpBtn = document.getElementById('reseller-verify-otp-btn');
                        const resellerResendOtpBtn = document.getElementById('reseller-resend-otp-btn');
                        const resellerOtpMsg = document.getElementById('reseller-otp-message');
                        const resellerOtpEmailInput = document.getElementById('reseller-otp-email');
                        const resellerEditEmailBtn = document.getElementById('reseller-edit-email-btn');
                        const resellerLoginBtnWrap = document.getElementById('reseller-login-btn-wrap');

                        function showResellerOtpSection(email) {
                            if (resellerLoginForm) resellerLoginEmail.disabled = true;
                            if (resellerLoginBtnWrap) resellerLoginBtnWrap.style.display = 'none';
                            if (resellerOtpSection) resellerOtpSection.style.display = 'block';
                            if (resellerOtpEmailInput) {
                                resellerOtpEmailInput.value = email;
                                resellerOtpEmailInput.disabled = true;
                                resellerOtpEmailInput.setAttribute('aria-disabled', 'true');
                                resellerEditEmailBtn.style.display = 'inline-block';
                            }
                            if (resellerOtpMsg) {
                                resellerOtpMsg.textContent = `Enter the 6-digit OTP sent to ${email}`;
                            }
                            if (resellerOtpInputs.length > 0) {
                                resellerOtpInputs.forEach(input => input.value = '');
                                resellerOtpInputs[0].focus();
                            }
                        }
                        function hideResellerOtpSection() {
                            if (resellerLoginEmail) resellerLoginEmail.disabled = false;
                            if (resellerOtpSection) resellerOtpSection.style.display = 'none';
                            if (resellerLoginBtnWrap) resellerLoginBtnWrap.style.display = 'block';
                            if (resellerOtpEmailInput) {
                                resellerOtpEmailInput.disabled = false;
                                resellerOtpEmailInput.removeAttribute('aria-disabled');
                            }
                        }

                        function temporarilyMakeFieldActiveAndNotRequired(input) {                        
                            let changedRequired = false;
                            if (input && (input.style.display === 'none' || input.disabled)) {
                                if (input.required) {
                                    input.required = false;
                                    changedRequired = true;
                                }
                                if (input.disabled) {
                                    input.disabled = false;
                                }
                                if (input.style.display === 'none') {
                                    input.style.display = 'block';
                                }
                            }
                            return changedRequired;
                        }
                        function restoreRequiredIfChanged(input, changedRequired) {
                            if (input && changedRequired) {
                                input.required = true;
                            }
                        }
                        // ---------------------------------------------------------------------

                        if (resellerLoginForm) {
                            resellerLoginForm.addEventListener('submit', async function(e) {
                     
                                let otpInput = document.getElementById('reseller-otp-email');
                                let changedRequired = false;
                                if (otpInput) {
                                    changedRequired = temporarilyMakeFieldActiveAndNotRequired(otpInput);
                                }
                   

                                e.preventDefault();
                                const email = resellerLoginEmail.value.trim();
                                if (!email) {
                                    Swal.fire({
                                        icon: "warning",
                                        title: "Missing Email",
                                        text: "Please enter your email address.",
                                        confirmButtonColor: '#b50957'
                                    });
                                    resellerLoginEmail.focus(); 
                                    restoreRequiredIfChanged(otpInput, changedRequired);
                                    return;
                                }
                                // Send login OTP
                                if (resellerSendOtpBtn) {
                                    resellerSendOtpBtn.disabled = true;
                                    resellerSendOtpBtn.innerHTML = '<span class="spinner-border spinner-border-sm"></span> Sending...';
                                }
                                try {
                                    const res = await fetch('/reseller/send-login-otp', {
                                        method: "POST",
                                        headers: { 'Content-Type': 'application/json' },
                                        body: JSON.stringify({ email })
                                    });
                                    const data = await res.json();
                                    if (res.ok && data.success) {
                                        Swal.fire({
                                            icon: "success",
                                            title: "OTP Sent!",
                                            text: "Check your email for the 6-digit OTP code.",
                                            timer: 2000,
                                            showConfirmButton: false
                                        });
                                        showResellerOtpSection(email);
                                    }
                                    else {
                                        Swal.fire({
                                            icon: "error",
                                            title: "Failed to Send OTP",
                                            text: data.message || "Could not send OTP. Please try again.",
                                            confirmButtonColor: '#b50957'
                                        });
                                    }
                                } catch (err) {
                                    Swal.fire({
                                        icon: "error",
                                        title: "Network Error",
                                        text: "Unable to send OTP. Please check your connection.",
                                        confirmButtonColor: '#b50957'
                                    });
                                } finally {
                                    resellerSendOtpBtn.disabled = false;
                                    resellerSendOtpBtn.innerHTML = '<span class="text text-button">Send OTP</span>';
                                    restoreRequiredIfChanged(otpInput, changedRequired);
                                }
                            });
                        }
                        // OTP boxes (auto move, keyboard paste)
                        if (resellerOtpInputs.length > 0) {
                            resellerOtpInputs.forEach((input, idx) => {
                                input.addEventListener('input', function(e) {
                                    let value = this.value.replace(/\D/g, '');
                                    this.value = value.slice(0, 1);
                                    if (value && idx < resellerOtpInputs.length - 1) {
                                        resellerOtpInputs[idx + 1].focus();
                                    }
                                    // auto-verify on last
                                    if (value && idx === resellerOtpInputs.length - 1) {
                                        const allFilled = Array.from(resellerOtpInputs).every(input => input.value);
                                        if (allFilled) setTimeout(()=>{ if (resellerVerifyOtpBtn) resellerVerifyOtpBtn.click()}, 300);
                                    }
                                });
                                input.addEventListener('keydown', function(e) {
                                    if (e.key === "Backspace" && !this.value && idx > 0) {
                                        resellerOtpInputs[idx-1].focus();
                                    }
                                    // Handle paste
                                    if (e.key === 'v' && (e.ctrlKey || e.metaKey)) {
                                        setTimeout(() => {
                                            const pastedText = this.value;
                                            if (pastedText.length === 6 && /^\d+$/.test(pastedText)) {
                                                for (let i = 0; i < 6; i++) {
                                                    if (resellerOtpInputs[i]) resellerOtpInputs[i].value = pastedText[i];
                                                }
                                                if (resellerVerifyOtpBtn) resellerVerifyOtpBtn.click();
                                            }
                                        }, 10);
                                    }
                                });
                                input.addEventListener('focus', function() { this.select(); });
                            });
                        }
                        // OTP "Verify" button
                        if (resellerVerifyOtpBtn) {
                            resellerVerifyOtpBtn.addEventListener('click', async function(e) {
                                e.preventDefault();

                                // PATCH: Make sure reseller-otp-email isn't hidden/disabled/required when validating
                                let emailInput = document.getElementById('reseller-otp-email');
                                let changedRequired = false;
                                if (emailInput) {
                                    changedRequired = temporarilyMakeFieldActiveAndNotRequired(emailInput);
                                }

                                let otp = "";
                                if (resellerOtpInputs.length > 0)
                                    resellerOtpInputs.forEach(input => otp += input.value.trim());
                                if (otp.length !== 6) {
                                    Swal.fire({
                                        icon: "warning",
                                        title: "Invalid OTP",
                                        text: "Please enter the complete 6-digit OTP.",
                                        confirmButtonColor: '#b50957'
                                    });
                                    restoreRequiredIfChanged(emailInput, changedRequired);
                                    return;
                                }
                                // Find email from disabled input or backup login field
                                const email = resellerOtpEmailInput.value.trim() || resellerLoginEmail.value.trim();
                                if (!email) {
                                    Swal.fire({
                                        icon: "warning",
                                        title: "Missing Email",
                                        text: "Please enter your email address.",
                                        confirmButtonColor: '#b50957'
                                    });
                                    restoreRequiredIfChanged(emailInput, changedRequired);
                                    return;
                                }
                                resellerVerifyOtpBtn.disabled = true;
                                resellerVerifyOtpBtn.innerHTML = '<span class="spinner-border spinner-border-sm"></span> Verifying...';
                                try {
                                    const res = await fetch('/reseller/verify-login-otp', {
                                        method: "POST",
                                        headers: { 'Content-Type': 'application/json' },
                                        body: JSON.stringify({ email, otp })
                                    });
                                    const data = await res.json();
                                    if (res.ok && data.success) {
                                        Swal.fire({
                                            icon: "success",
                                            title: "Login Successful!",
                                            text: "Redirecting to your reseller dashboard...",
                                            timer: 1500,
                                            showConfirmButton: false
                                        }).then(()=> {
                                            window.location.href = "/reseller/reseller_dashboard";
                                        });
                                    } else {
                                        Swal.fire({
                                            icon: "error",
                                            title: "OTP Verification Failed",
                                            text: (data && data.message) ? data.message : "Invalid or expired OTP. Please try again.",
                                            confirmButtonColor: '#b50957'
                                        });
                                    }
                                } catch (err) {
                                    Swal.fire({
                                        icon: "error",
                                        title: "Network Error",
                                        text: "Unable to verify OTP. Please check your internet connection.",
                                        confirmButtonColor: '#b50957'
                                    });
                                } finally {
                                    resellerVerifyOtpBtn.disabled = false;
                                    resellerVerifyOtpBtn.innerHTML = "Verify OTP";
                                    restoreRequiredIfChanged(emailInput, changedRequired);
                                }
                            });
                        }
                        // OTP "Resend" button
                        if (resellerResendOtpBtn) {
                            resellerResendOtpBtn.addEventListener('click', async function(e) {
                                e.preventDefault();

                                // PATCH: Make sure reseller-otp-email isn't hidden/disabled/required
                                let emailInput = document.getElementById('reseller-otp-email');
                                let changedRequired = false;
                                if (emailInput) {
                                    changedRequired = temporarilyMakeFieldActiveAndNotRequired(emailInput);
                                }

                                const email = resellerOtpEmailInput.value.trim() || resellerLoginEmail.value.trim();
                                if (!email) {
                                    Swal.fire({
                                        icon: "warning",
                                        title: "Missing Email",
                                        text: "Please enter your email address.",
                                        confirmButtonColor: '#b50957'
                                    });
                                    restoreRequiredIfChanged(emailInput, changedRequired);
                                    return;
                                }
                                resellerResendOtpBtn.disabled = true;
                                resellerResendOtpBtn.innerHTML = '<span class="spinner-border spinner-border-sm"></span> Resending...';
                                try {
                                    const res = await fetch('/reseller/send-login-otp', {
                                        method: "POST",
                                        headers: { 'Content-Type': 'application/json' },
                                        body: JSON.stringify({ email })
                                    });
                                    const data = await res.json();
                                    if (res.ok && data.success) {
                                        Swal.fire({
                                            icon: "success",
                                            title: "OTP Resent!",
                                            text: "A new OTP has been sent to your email.",
                                            confirmButtonColor: '#b50957'
                                        });
                                        if (resellerOtpInputs.length > 0) {
                                            resellerOtpInputs.forEach(input => input.value = '');
                                            resellerOtpInputs[0].focus();
                                        }
                                    } else {
                                        Swal.fire({
                                            icon: "error",
                                            title: "Failed to Resend OTP",
                                            text: data.message || "Could not resend OTP. Please try again.",
                                            confirmButtonColor: '#b50957'
                                        });
                                    }
                                } catch (err) {
                                    Swal.fire({
                                        icon: "error",
                                        title: "Resend Error",
                                        text: "Unable to resend OTP. Please try again.",
                                        confirmButtonColor: '#b50957'
                                    });
                                } finally {
                                    resellerResendOtpBtn.disabled = false;
                                    resellerResendOtpBtn.innerHTML = "Resend OTP";
                                    restoreRequiredIfChanged(emailInput, changedRequired);
                                }
                            });
                        }
                        // Edit email in OTP section
                        if (resellerEditEmailBtn) {
                            resellerEditEmailBtn.addEventListener('click', function() {
                                if (resellerOtpEmailInput) {
                                    resellerOtpEmailInput.disabled = false;
                                    resellerOtpEmailInput.removeAttribute('aria-disabled');
                                    resellerOtpEmailInput.focus();
                                    resellerEditEmailBtn.style.display = 'none';
                                }
                            });
                            // Save (on blur) disables again for safety
                            resellerOtpEmailInput.addEventListener('blur', function() {
                                resellerOtpEmailInput.disabled = true;
                                resellerOtpEmailInput.setAttribute('aria-disabled', 'true');
                                resellerEditEmailBtn.style.display = 'inline-block';
                            });
                        }
                        // Show only login by default
                        hideResellerOtpSection();
                        </script>

                      
                    </div>
                </div>
            </div>
        </section>
