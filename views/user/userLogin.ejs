                        <form action="#" class="form-login form-has-password login-form active" id="customer-login" autocomplete="off">
                            <div class="wrap">
                                <fieldset>
                                    <input type="text" name="name" id="user-name" placeholder="Your Name*" required />
                                </fieldset>

                                <fieldset>
                                    <input type="email" id="user-email" name="email"
                                           placeholder="Email Address*" required autocomplete="username" />
                                </fieldset>

                                <button type="button" class="tf-btn btn-fill" style="width: 100%!important;" id="send-otp-btn">
                                    Send OTP
                                </button>

                                <fieldset id="otpField" style="display:none; margin-top:15px;">
                                    <input type="text" id="otpInput" maxlength="6" placeholder="Enter OTP*" autocomplete="one-time-code" />
                                </fieldset>

                                <button id="verifyBtn" type="button" class="tf-btn btn-fill"
                                        style="width: 100%!important; margin-top:10px; display:none;">
                                    Verify OTP
                                </button>
                            </div>
                        </form>   

                        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
                        <script>
                            // Helper function to validate email
                            function validateEmail(email) {
                                return /^[^\s@]+@[^\s@]+\.[^\s@]+$/.test(email);
                            }
                            // Send OTP handler for customer login
                            document.getElementById('send-otp-btn').onclick = async function() {
                                const name = document.getElementById('user-name').value.trim();
                                const email = document.getElementById('user-email').value.trim();

                                if (!name || !email) {
                                    Swal.fire({ icon: 'warning', text: 'Please provide both Name and Email.' });
                                    return;
                                }
                                if (!validateEmail(email)) {
                                    Swal.fire({ icon: 'warning', text: 'Enter a valid email address.' });
                                    return;
                                }

                                this.disabled = true;
                                this.innerText = "Sending...";

                                try {
                                    const res = await fetch('/user/send-otp', {
                                        method: 'POST',
                                        headers: { 'Content-Type': 'application/json' },
                                        body: JSON.stringify({ name, email })
                                    });
                                    const data = await res.json();

                                    if (data.success) {
                                        Swal.fire({ icon: "success", text: data.message });
                                        document.getElementById('otpField').style.display = '';
                                        document.getElementById('verifyBtn').style.display = '';
                                        document.getElementById('user-name').readOnly = true;
                                        document.getElementById('user-email').readOnly = true;
                                        this.style.display = 'none';
                                    } else {
                                        Swal.fire({ icon: "error", text: data.message || "Failed to send OTP" });
                                        this.disabled = false;
                                        this.innerText = "Send OTP";
                                    }
                                } catch (e) {
                                    Swal.fire({ icon: "error", text: "Server error sending OTP" });
                                    this.disabled = false;
                                    this.innerText = "Send OTP";
                                }
                            };

                            // Verify OTP handler for customer login
                            document.getElementById('verifyBtn').onclick = async function() {
                                const email = document.getElementById('user-email').value.trim();
                                const otp = (document.getElementById('otpInput').value || '').trim();

                                if (!otp || otp.length !== 6) {
                                    Swal.fire({ icon: 'warning', text: "Please enter the 6-digit OTP." });
                                    return;
                                }

                                this.disabled = true;
                                this.innerText = "Verifying...";

                                try {
                                    const res = await fetch('/user/verify-otp', {
                                        method: 'POST',
                                        headers: { 'Content-Type': 'application/json' },
                                        body: JSON.stringify({ email, otp })
                                    });
                                    const data = await res.json();

                                    if (data.success) {
                                        Swal.fire({
                                            icon: "success",
                                            text: data.message || "Logged in successfully"
                                        }).then(() => {
                                            window.location.href = data.redirect || "/";
                                        });
                                    } else {
                                        Swal.fire({ icon: "error", text: data.message || "OTP verification failed" });
                                        document.getElementById('otpInput').value = '';
                                        this.disabled = false;
                                        this.innerText = "Verify OTP";
                                    }
                                } catch (e) {
                                    Swal.fire({ icon: "error", text: "Server error verifying OTP" });
                                    this.disabled = false;
                                    this.innerText = "Verify OTP";
                                }
                            };
                        </script>
              
    
                        <!-- Salon Login Form -->
                        <form action="#" class="form-login form-has-password login-form" id="salon-login" autocomplete="off">
                            <div class="wrap">
                                <fieldset class="">
                                    <input type="text" name="username" id="salon-username" placeholder="Salon Username*" tabindex="2"
                                        aria-required="true" required />
                                </fieldset>
                                <fieldset class="position-relative password-item">
                                    <input type="password" name="password" id="salon-password" placeholder="Password*" tabindex="2"
                                        aria-required="true" required />
                                    <span class="toggle-password unshow">
                                        <i class="icon-eye-hide-line"></i>
                                    </span>
                                </fieldset>
                                <div class="d-flex align-items-center justify-content-between">
                                    <div class="tf-cart-checkbox">
                                        <div class="tf-checkbox-wrapp">
                                            <input checked class="" type="checkbox" id="salon-login-form_agree"
                                                name="agree_checkbox">
                                            <div>
                                                <i class="icon-check"></i>
