                        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
                        <script>
                            document.getElementById('salon-login').addEventListener('submit', async function(e) {
                                e.preventDefault();

                                const username = document.getElementById('salon-username').value.trim();
                                const password = document.getElementById('salon-password').value;

                                if (!username || !password) {
                                    Swal.fire({
                                        icon: 'error',
                                        title: 'Missing credentials',
                                        text: 'Please provide both username and password.',
                                        confirmButtonColor: '#e63946'
                                    });
                                    return;
                                }

                                try {
                                    const res = await fetch('/salon/salonlogin', {
                                        method: 'POST',
                                        headers: {
                                            'Content-Type': 'application/json'
                                        },
                                        body: JSON.stringify({
                                            username,
                                            password
                                        })
