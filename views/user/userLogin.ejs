                        <script>
                      // Freelance Beautician Login Script - CORRECTED VERSION
document.addEventListener('DOMContentLoaded', function () {
    console.log('Initializing freelance login...');
    
    // Elements - with null checks
    const freelanceLoginForm = document.getElementById('freelance-login-form');
    const freelanceLoginEmail = document.getElementById('freelance-login-email');
    const sendOtpBtn = document.getElementById('freelance-send-otp-btn');
    const otpSection = document.getElementById('freelance-otp-section');
    const otpInputs = otpSection ? otpSection.querySelectorAll('.otp-input') : [];
    const verifyOtpBtn = document.getElementById('freelance-verify-otp-btn');
    const resendOtpBtn = document.getElementById('freelance-resend-otp-btn');
    const otpMsg = document.getElementById('freelance-otp-message');
    const otpTitle = document.getElementById('freelance-otp-title');
    const otpEmailInputWrap = document.getElementById('freelance-otp-email-input-wrap');
    const otpEmailInput = document.getElementById('freelance-otp-email');
    const editOtpEmailBtn = document.getElementById('freelance-edit-email-btn');


    // Helper functions
    function safeShow(elem) {
        if (elem) elem.style.display = 'block';
    }

    function safeHide(elem) {
        if (elem) elem.style.display = 'none';
    }

    // Show OTP section and fill the OTP email input
    function showOtpSection(email) {
        // Hide the email form
        safeHide(freelanceLoginForm);
        
        // Show the OTP section
        safeShow(otpSection);
        
        // Set the email in OTP section
        if (otpEmailInput) {
            otpEmailInput.value = email || '';
            otpEmailInput.disabled = false;
        }
        
        // Update OTP message
        if (otpMsg) {
            otpMsg.textContent = 'Please enter the 6-digit OTP sent to your email: ' + (email || '');
        }
        
        // Clear and focus first OTP input
        if (otpInputs.length > 0) {
            otpInputs.forEach(input => input.value = '');
            otpInputs[0].focus();
        }
        
        // Hide edit button (email is editable directly)
        safeHide(editOtpEmailBtn);
    }

    function showLoginForm() {
        safeShow(freelanceLoginForm);
        safeHide(otpSection);
        
        // Clear OTP inputs when going back to login
        if (otpInputs.length > 0) {
            otpInputs.forEach(input => input.value = '');
        }
    }

    function setLoading(btn, isLoading, text) {
        if (!btn) return;
        
        if (isLoading) {
            btn.dataset.origText = btn.innerHTML;
            btn.disabled = true;
            btn.innerHTML = '<span class="spinner-border spinner-border-sm" style="vertical-align:middle;"></span> ' + (text || 'Loading...');
        } else {
            if (btn.dataset.origText) {
                btn.innerHTML = btn.dataset.origText;
            }
            btn.disabled = false;
        }
    }

    // Submit: Send OTP
    if (freelanceLoginForm) {
        freelanceLoginForm.addEventListener('submit', async function (e) {
            e.preventDefault();
        
            const email = freelanceLoginEmail ? freelanceLoginEmail.value.trim() : '';
            
            if (!email) {
                Swal.fire({
                    icon: 'warning',
                    title: 'Missing Email',
                    text: 'Please enter your email address.'
                });
                return;
            }

            setLoading(sendOtpBtn, true, 'Sending OTP...');
            
            try {
                const res = await fetch('/freelance/send-login-otp', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({ email })
                });
                
                const data = await res.json();
                setLoading(sendOtpBtn, false);
                
                if (res.ok && data.success) {
                    Swal.fire({
                        icon: 'success',
                        title: 'OTP Sent!',
                        text: 'Check your email for the 6-digit OTP code.',
                        timer: 2000,
                        showConfirmButton: false
                    });
                    
                    // Show OTP section after successful OTP send
                    showOtpSection(email);
                    
                } else {
                    Swal.fire({
                        icon: 'error',
                        title: 'Failed to Send OTP',
                        text: data.message || 'Could not send OTP. Please try again.'
                    });
                }
            } catch (err) {
                setLoading(sendOtpBtn, false);
                console.error('OTP send error:', err);
                Swal.fire({
                    icon: 'error',
                    title: 'Network Error',
                    text: 'Unable to send OTP. Please check your connection.'
                });
            }
        });
    }

    // OTP Input UX - Auto-focus and navigation
    if (otpInputs.length > 0) {
        otpInputs.forEach((input, idx) => {
            input.addEventListener('input', function (e) {
                const value = this.value;
                
                // Only allow numbers
                if (value && !/^\d$/.test(value)) {
                    this.value = '';
                    return;
                }
                
                // Auto-focus next input
                if (value && idx < otpInputs.length - 1) {
                    otpInputs[idx + 1].focus();
                }
                
                // Auto-verify when last digit is entered
                if (value && idx === otpInputs.length - 1) {
                    const allFilled = Array.from(otpInputs).every(input => input.value);
                    if (allFilled) {
                        // Small delay to let user see the last digit
                        setTimeout(() => {
                            if (verifyOtpBtn) verifyOtpBtn.click();
                        }, 300);
                    }
                }
            });
            
            input.addEventListener('keydown', function (e) {
                // Handle backspace
                if (e.key === 'Backspace' && !this.value && idx > 0) {
                    otpInputs[idx - 1].focus();
                }
                
                // Handle paste
                if (e.key === 'v' && (e.ctrlKey || e.metaKey)) {
                    setTimeout(() => {
                        const pastedText = this.value;
                        if (pastedText.length === 6 && /^\d+$/.test(pastedText)) {
                            for (let i = 0; i < 6; i++) {
                                if (otpInputs[i]) {
                                    otpInputs[i].value = pastedText[i];
                                }
                            }
                            if (verifyOtpBtn) verifyOtpBtn.click();
                        }
                    }, 10);
                }
            });
            
            input.addEventListener('focus', function () {
                this.select();
            });
        });
    }

    // Verify OTP
    if (verifyOtpBtn) {
        verifyOtpBtn.addEventListener('click', async function (e) {
            e.preventDefault();
          
            // Get OTP from inputs
            let otp = '';
            if (otpInputs.length > 0) {
                otpInputs.forEach(input => otp += input.value.trim());
            }
            
            if (otp.length !== 6) {
                Swal.fire({
                    icon: 'warning',
                    title: 'Invalid OTP',
                    text: 'Please enter the complete 6-digit OTP.'
                });
                return;
            }
            
            // Get email from OTP section or login form
            const email = otpEmailInput ? otpEmailInput.value.trim() : 
                         (freelanceLoginEmail ? freelanceLoginEmail.value.trim() : '');
            
            if (!email) {
                Swal.fire({
                    icon: 'warning',
                    title: 'Missing Email',
                    text: 'Please enter your email address.'
                });
                return;
            }

            setLoading(verifyOtpBtn, true, 'Verifying...');
            
            try {
                const response = await fetch('/freelance/verify-login-otp', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({ email, otp })
                });
                
                const result = await response.json();
                setLoading(verifyOtpBtn, false);
                
                if (response.ok && result.success) {
                    Swal.fire({
                        icon: 'success',
                        title: 'Login Successful!',
                        text: 'Redirecting to your dashboard...',
                        timer: 1500,
                        showConfirmButton: false
                    }).then(() => {
                        // Redirect to dashboard
                        window.location.href = result.redirect || result.redirectTo || '/freelance/beautician_dashboard';
                    });
                } else {
                    Swal.fire({
                        icon: 'error',
                        title: 'OTP Verification Failed',
                        text: result.message || 'Invalid OTP. Please try again.'
                    });
                    
                    // Clear OTP inputs and refocus
                    if (otpInputs.length > 0) {
                        otpInputs.forEach(input => input.value = '');
                        otpInputs[0].focus();
                    }
                }
            } catch (err) {
                setLoading(verifyOtpBtn, false);
                console.error('OTP verification error:', err);
                Swal.fire({
                    icon: 'error',
                    title: 'Verification Error',
                    text: 'Unable to verify OTP. Please try again.'
                });
                
                // Clear OTP inputs
                if (otpInputs.length > 0) {
                    otpInputs.forEach(input => input.value = '');
                    otpInputs[0].focus();
                }
            }
        });
    }

    // Resend OTP
    if (resendOtpBtn) {
        resendOtpBtn.addEventListener('click', async function () {
            console.log('Resending OTP...');
            
            // Get email from OTP section or login form
            const email = otpEmailInput ? otpEmailInput.value.trim() : 
                         (freelanceLoginEmail ? freelanceLoginEmail.value.trim() : '');
            
            if (!email) {
                Swal.fire({
                    icon: 'warning',
                    title: 'Missing Email',
                    text: 'Please enter your email address.'
                });
                return;
            }

            setLoading(resendOtpBtn, true, 'Resending...');
            
            try {
                const res = await fetch('/freelance/send-login-otp', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({ email })
                });
                
                const data = await res.json();
                setLoading(resendOtpBtn, false);
                
                if (res.ok && data.success) {
                    Swal.fire({
                        icon: 'success',
                        title: 'OTP Resent!',
                        text: 'New OTP has been sent to your email.',
                        timer: 1500,
                        showConfirmButton: false
                    });
                    
                    // Update OTP message
                    if (otpMsg) {
                        otpMsg.textContent = 'Please enter the 6-digit OTP sent to your email: ' + email;
                    }
                    
                    // Clear and refocus OTP inputs
                    if (otpInputs.length > 0) {
                        otpInputs.forEach(input => input.value = '');
                        otpInputs[0].focus();
                    }
                } else {
                    Swal.fire({
                        icon: 'error',
                        title: 'Failed to Resend OTP',
                        text: data.message || 'Could not resend OTP. Please try again.'
                    });
                }
            } catch (err) {
                setLoading(resendOtpBtn, false);
                console.error('Resend OTP error:', err);
                Swal.fire({
                    icon: 'error',
                    title: 'Resend Error',
                    text: 'Unable to resend OTP. Please try again.'
                });
            }
        });
    }

    // Edit email in OTP section
    if (editOtpEmailBtn) {
        editOtpEmailBtn.addEventListener('click', function () {
            if (otpEmailInput) {
                otpEmailInput.disabled = false;
                otpEmailInput.focus();
                safeHide(editOtpEmailBtn);
            }
        });
    }

    // Initialize - ensure OTP section is hidden on page load
    if (otpSection) {
        otpSection.style.display = 'none';
    }
    
    // Ensure login form is visible
    if (freelanceLoginForm) {
        freelanceLoginForm.style.display = 'block';
    }

    console.log('Freelance login initialization complete');
});
                        </script>

