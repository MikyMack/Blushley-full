        <script>
            // Login type selector functionality - CORRECTED VERSION
            document.addEventListener('DOMContentLoaded', function () {
                const loginTypeBtns = document.querySelectorAll('.login-type-btn');
                const loginForms = document.querySelectorAll('.login-form');
                const sellerRegisterPanel = document.getElementById('reseller-register-panel');
                const sellerLoginForm = document.getElementById('reseller-login-form');
                const openSellerRegisterBtn = document.getElementById('openResellerRegister');
                const backToSellerLoginBtn = document.getElementById('backToResellerLogin');
            
                // Form ID mapping for each login type - CORRECTED
                const formIds = {
                    'customer': 'customer-login',
                    'salon': 'salon-login',
                    'freelance': 'freelance-login-form',
                    'seller': 'reseller-login-form'  // Fixed: was 'seller-login', now 'reseller-login-form'
                };
            
                loginTypeBtns.forEach(btn => {
                    btn.addEventListener('click', function () {
                        // Remove active class from all buttons and forms
                        loginTypeBtns.forEach(b => b.classList.remove('active'));
                        loginForms.forEach(form => {
                            form.classList.remove('active');
                            form.style.display = 'none'; // Hide all forms
                        });
            
                        // Add active class to clicked button
                        this.classList.add('active');
            
                        // Show corresponding form
                        const loginType = this.getAttribute('data-type');
                        const formId = formIds[loginType];
                        
                        console.log('Switching to:', loginType, 'Form ID:', formId); // Debug log
                        
                        if (formId) {
                            const formEl = document.getElementById(formId);
                            if (formEl) {
                                formEl.classList.add('active');
                                formEl.style.display = 'block'; // Show the form
                                
                                // Special handling for freelance form - ensure OTP section is hidden initially
                                if (loginType === 'freelance') {
                                    const otpSection = document.getElementById('freelance-otp-section');
                                    if (otpSection) {
                                        otpSection.style.display = 'none';
                                    }
                                    const freelanceForm = document.getElementById('freelance-login-form');
                                    if (freelanceForm) {
                                        freelanceForm.style.display = 'block';
                                    }
                                }
                                
                                // Special handling for reseller form - ensure OTP section is hidden initially
                                if (loginType === 'seller') {
                                    const otpSection = document.getElementById('reseller-otp-section');
                                    if (otpSection) {
                                        otpSection.style.display = 'none';
                                    }
                                    const resellerLoginBtnWrap = document.getElementById('reseller-login-btn-wrap');
                                    if (resellerLoginBtnWrap) {
                                        resellerLoginBtnWrap.style.display = 'block';
                                    }
                                }
                            } else {
                                console.error('Form element not found:', formId); // Debug log
                            }
                        }
            
                        // When switching type, ensure seller register panel is hidden and seller login is visible
                        if (sellerRegisterPanel) {
                            sellerRegisterPanel.classList.remove('active');
                            sellerRegisterPanel.style.display = 'none';
                        }
                        if (sellerLoginForm) {
                            sellerLoginForm.style.display = loginType === 'seller' ? 'block' : 'none';
                        }
                    });
                });
            
                // Initialize - show customer form by default
                const customerBtn = document.querySelector('.login-type-btn[data-type="customer"]');
                if (customerBtn) {
                    customerBtn.click(); // Trigger click to show customer form
                }
            
                // Form validation for basic forms (customer, salon)
                document.querySelectorAll('.login-form').forEach(form => {
                    // Skip reseller form as it has its own OTP logic
                    if (form.id === 'reseller-login-form') return;
                    
                    form.addEventListener('submit', function (e) {
                        e.preventDefault();
            
                        // Get all required inputs in this form
                        const requiredInputs = this.querySelectorAll('input[required], select[required]');
                        let isValid = true;
            
                        requiredInputs.forEach(input => {
                            if (!input.value.trim()) {
                                isValid = false;
                                input.style.borderColor = 'red';
                            } else {
                                input.style.borderColor = '#e0e0e0';
                            }
                        });
            
                        if (isValid) {
                            const formType = this.id.split('-')[0];
                        
                        } else {
                            alert('Please fill in all required fields.');
                        }
                    });
                });
            
                // Seller Register toggle
                if (openSellerRegisterBtn && backToSellerLoginBtn && sellerRegisterPanel && sellerLoginForm) {
                    openSellerRegisterBtn.addEventListener('click', function () {
                        sellerRegisterPanel.classList.add('active');
                        sellerRegisterPanel.style.display = 'block';
                        sellerLoginForm.style.display = 'none';
                    });
            
                    backToSellerLoginBtn.addEventListener('click', function () {
                        sellerRegisterPanel.classList.remove('active');
                        sellerRegisterPanel.style.display = 'none';
                        sellerLoginForm.style.display = 'block';
                    });
                }
            
                // Note: The reseller OTP login logic is already included in your separate script
                // so we don't need to duplicate it here
            });
            </script>
    
        <script>
        document.addEventListener('DOMContentLoaded', function() {
            const navAccount = document.querySelector('.nav-account');
            if (!navAccount) return;
            const navIconItem = navAccount.querySelector('.nav-icon-item');
            if (!navIconItem) return;

            // Toggle dropdown on click
            navIconItem.addEventListener('click', function(e) {
                e.preventDefault();
                e.stopPropagation();
                navAccount.classList.toggle('show-dropdown');
            });

            // Close dropdown when clicking outside
            document.addEventListener('click', function(e) {
                if (!navAccount.contains(e.target)) {
                    navAccount.classList.remove('show-dropdown');
                }
            });

            // Prevent dropdown from closing when clicking inside it
            const dropdown = navAccount.querySelector('.dropdown-account');
            if (dropdown) {
                dropdown.addEventListener('click', function(e) {
                    e.stopPropagation();
                });
            }
        });
        </script>

