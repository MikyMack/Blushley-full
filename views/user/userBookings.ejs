                status: 'cancelled',
                stylist: 'Not Assigned'
            }
        ];

        // Initialize
        document.addEventListener('DOMContentLoaded', function() {
            // Simulate API call delay
            setTimeout(() => {
                renderBookings('all');
                document.getElementById('sbmLoading').style.display = 'none';
            }, 1000);

            // Setup filter tabs
            document.querySelectorAll('.sbm-tab-btn').forEach(btn => {
                btn.addEventListener('click', function() {
                    document.querySelectorAll('.sbm-tab-btn').forEach(b => b.classList.remove('sbm-active'));
                    this.classList.add('sbm-active');
                    renderBookings(this.dataset.filter);
                });
            });
        });

        function renderBookings(filter) {
            const grid = document.getElementById('sbmBookingsGrid');
            const emptyState = document.getElementById('sbmEmptyState');
            
            let filteredBookings = sampleBookings;
            
            if (filter !== 'all') {
                filteredBookings = sampleBookings.filter(b => b.status === filter);
            }

            if (filteredBookings.length === 0) {
                grid.innerHTML = '';
                emptyState.style.display = 'block';
                return;
            }

            emptyState.style.display = 'none';
            
            grid.innerHTML = filteredBookings.map(booking => `
                <div class="sbm-booking-card">
                    <div class="sbm-booking-header">
                        <div class="sbm-salon-info">
                            <h3>${booking.salon}</h3>
                            <span class="sbm-booking-id">Booking ID: ${booking.id}</span>
                        </div>
                        <span class="sbm-status-badge sbm-status-${booking.status}">${booking.status}</span>
                    </div>

                    <div class="sbm-booking-details">
                        <div class="sbm-detail-item">
                            <div class="sbm-detail-icon">ðŸ“…</div>
                            <div class="sbm-detail-content">
                                <h4>Date & Time</h4>
                                <p>${formatDate(booking.date)} at ${booking.time}</p>
                            </div>
                        </div>
                        <div class="sbm-detail-item">
                            <div class="sbm-detail-icon">ðŸ“</div>
                            <div class="sbm-detail-content">
                                <h4>Location</h4>
                                <p>${booking.location}</p>
                            </div>
                        </div>
                        <div class="sbm-detail-item">
                            <div class="sbm-detail-icon">ðŸ‘¤</div>
                            <div class="sbm-detail-content">
                                <h4>Stylist</h4>
                                <p>${booking.stylist}</p>
                            </div>
                        </div>
                        <div class="sbm-detail-item">
                            <div class="sbm-detail-icon">ðŸ’°</div>
                            <div class="sbm-detail-content">
                                <h4>Total Amount</h4>
                                <p>â‚¹${booking.amount}</p>
                            </div>
                        </div>
                    </div>

                    <div class="sbm-services-list">
                        <h4>Services Booked</h4>
                        <div class="sbm-service-tags">
                            ${booking.services.map(s => `<span class="sbm-service-tag">${s}</span>`).join('')}
                        </div>
                    </div>

                    <div class="sbm-booking-actions">
                        ${getActionButtons(booking.status, booking.id)}
                    </div>
                </div>
            `).join('');
        }

        function getActionButtons(status, bookingId) {
            if (status === 'upcoming') {
                return `
                    <button class="sbm-action-btn sbm-btn-primary" onclick="viewDetails('${bookingId}')">View Details</button>
                    <button class="sbm-action-btn sbm-btn-secondary" onclick="reschedule('${bookingId}')">Reschedule</button>
                    <button class="sbm-action-btn sbm-btn-danger" onclick="cancelBooking('${bookingId}')">Cancel</button>
                `;
            } else if (status === 'completed') {
                return `
                    <button class="sbm-action-btn sbm-btn-primary" onclick="viewDetails('${bookingId}')">View Details</button>
                    <button class="sbm-action-btn sbm-btn-secondary" onclick="rebook('${bookingId}')">Book Again</button>
                    <button class="sbm-action-btn sbm-btn-secondary" onclick="leaveReview('${bookingId}')">Leave Review</button>
                `;
            } else {
                return `
                    <button class="sbm-action-btn sbm-btn-primary" onclick="viewDetails('${bookingId}')">View Details</button>
                    <button class="sbm-action-btn sbm-btn-secondary" onclick="rebook('${bookingId}')">Book Again</button>
                `;
            }
        }

        function formatDate(dateString) {
            const date = new Date(dateString);
            const options = { day: 'numeric', month: 'short', year: 'numeric' };
            return date.toLocaleDateString('en-IN', options);
        }

        // Action functions
        function viewDetails(bookingId) {
            alert(`Viewing details for booking: ${bookingId}`);
            // In production: Navigate to booking details page
        }

        function reschedule(bookingId) {
            alert(`Rescheduling booking: ${bookingId}`);
            // In production: Open reschedule modal
        }

        function cancelBooking(bookingId) {
            if (confirm('Are you sure you want to cancel this booking?')) {
                alert(`Booking ${bookingId} cancelled successfully`);
                // In production: Make API call to cancel booking
            }
        }

        function rebook(bookingId) {
            alert(`Rebooking services from: ${bookingId}`);
            // In production: Open booking modal with pre-filled services
        }

        function leaveReview(bookingId) {
            alert(`Leaving review for booking: ${bookingId}`);
            // In production: Open review modal
        }
    </script>

    <script>
    document.addEventListener('DOMContentLoaded', function() {
    const navAccount = document.querySelector('.nav-account');
    const navIconItem = navAccount.querySelector('.nav-icon-item');
    
    // Toggle dropdown on click
    navIconItem.addEventListener('click', function(e) {
        e.preventDefault();
        e.stopPropagation();
        navAccount.classList.toggle('show-dropdown');
    });
    
    // Close dropdown when clicking outside
    document.addEventListener('click', function(e) {
        if (!navAccount.contains(e.target)) {
            navAccount.classList.remove('show-dropdown');
        }
    });
    
    // Prevent dropdown from closing when clicking inside it
    const dropdown = navAccount.querySelector('.dropdown-account');
    dropdown.addEventListener('click', function(e) {
        e.stopPropagation();
    });
});

</script>

  
