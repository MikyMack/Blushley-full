                '380001': { area: 'Ahmedabad GPO', deliveryDays: '4-5', cod: true },
            };

            const pincodeInput = document.getElementById('laPincodeInput');
            const checkBtn = document.getElementById('laCheckBtn');
            const resultBox = document.getElementById('laResultBox');

            // Only allow numbers
            pincodeInput.addEventListener('input', function (e) {
                this.value = this.value.replace(/[^0-9]/g, '');
            });

            // Check on button click
            checkBtn.addEventListener('click', checkPincode);

            // Check on Enter key
            pincodeInput.addEventListener('keypress', function (e) {
                if (e.key === 'Enter') {
                    checkPincode();
                }
            });

            function checkPincode() {
                const pincode = pincodeInput.value.trim();

                // Validation
                if (pincode.length !== 6) {
                    showResult('error', 'Invalid Pincode', 'Please enter a valid 6-digit pincode');
                    return;
                }

                // Show loading state
                showResult('checking', 'Checking...', 'Please wait while we verify your location');
                checkBtn.disabled = true;

                // Simulate API call delay
                setTimeout(() => {
                    checkBtn.disabled = false;

                    if (serviceablePincodes[pincode]) {
                        const info = serviceablePincodes[pincode];
                        showResult(
                            'available',
                            'âœ“ Delivery Available!',
                            `Great news! We deliver to ${info.area}.`,
                            {
                                'Estimated Delivery': `${info.deliveryDays} business days`,
                                'Cash on Delivery': info.cod ? 'Available' : 'Not Available',
                                'Pincode': pincode
                            }
                        );
                    } else {
                        showResult(
                            'unavailable',
                            'âœ— Service Unavailable',
                            `Sorry, we currently don't deliver to pincode ${pincode}. We're expanding soon!`
                        );
                    }
                }, 1500);
            }

            function showResult(type, title, description, deliveryInfo = null) {
                resultBox.className = `la-result-box la-${type} la-show`;

                let icon = '';
                if (type === 'available') icon = 'âœ“';
                else if (type === 'unavailable') icon = 'âœ—';
                else if (type === 'checking') icon = '<span class="la-spinner"></span>';
                else if (type === 'error') icon = 'âš ';

                let deliveryInfoHTML = '';
                if (deliveryInfo) {
                    deliveryInfoHTML = '<div class="la-delivery-info">';
                    for (let [label, value] of Object.entries(deliveryInfo)) {
                        deliveryInfoHTML += `
                        <div class="la-delivery-info-item">
                            <span class="la-delivery-info-label">${label}:</span>
                            <span class="la-delivery-info-value">${value}</span>
                        </div>
                    `;
                    }
                    deliveryInfoHTML += '</div>';
                }

                resultBox.innerHTML = `
                <div class="la-result-content">
                    <div class="la-result-icon">${icon}</div>
                    <div class="la-result-text">
                        <div class="la-result-title">${title}</div>
                        <div class="la-result-description">${description}</div>
                        ${deliveryInfoHTML}
                    </div>
                </div>
            `;
            }
        </script>

        <script>  if (document.querySelector("#sib-form")) { const script = document.createElement('script'); script.src = 'js/sibforms.js'; script.defer = true; document.body.appendChild(script); }</script>
</body>



