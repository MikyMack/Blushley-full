        <section class="flat-spacing">
            <div class="container">
                <div class="heading-section text-center wow fadeInUp">
                    <h3 class="heading">Trendy Collection</h3>
                    <p class="subheading text-secondary">Fresh styles just in! Elevate your look.</p>
                </div>
                <% if (Array.isArray(products) && products.length) { %>
                <div dir="ltr" class="swiper tf-sw-recent"
                    data-preview="4" data-tablet="3" data-mobile="2"
                    data-space-lg="30" data-space-md="30" data-space="15"
                    data-pagination="1" data-pagination-md="1" data-pagination-lg="1">
                    <div class="swiper-wrapper">
                        <% products.forEach(function(product, index){ 
                            let base = typeof product.adminBasePrice === 'number' && product.adminBasePrice > 0
                                ? product.adminBasePrice
                                : product.basePrice;
                            let sale = typeof product.adminSalePrice === 'number' && product.adminSalePrice > 0
                                ? product.adminSalePrice
                                : (typeof product.salePrice === 'number' && product.salePrice > 0
                                    ? product.salePrice
                                    : null);

                            let discount = null;
                            if (base && sale && sale < base) {
                              discount = Math.round(((base - sale) / base) * 100);
                            }
                            let img1 = (product.images && product.images[0]) ? product.images[0] : '/images/no-image.png';
                            let img2 = (product.images && product.images[1]) ? product.images[1] : img1;
                        %>
                        <div class="swiper-slide">
                            <div class="card-product card-product-size wow fadeInUp" data-wow-delay="<%= (index * 0.1).toFixed(1) %>s">
                                <div class="card-product-wrapper">
                                    <a href="/productDetails/<%= product.slug %>" class="product-img">
                                        <img class="lazyload img-product"
                                            data-src="<%= img1 %>"
                                            src="<%= img1 %>" alt="<%= product.title %>">
                                        <img class="lazyload img-hover"
                                            data-src="<%= img2 %>"
                                            src="<%= img2 %>" alt="<%= product.title %>">
                                    </a>
                                    <% if (discount) { %>
                                    <div class="on-sale-wrap"><span class="on-sale-item">-<%= discount %>%</span></div>
                                    <% } %>
                                    <div class="list-btn-main">
                                        <a href="/productDetails/<%= product.slug %>" class="btn-main-product">Shop Now</a>
                                    </div>
                                </div>
                                <div class="card-product-info">
