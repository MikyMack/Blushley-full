        <script>
            // Review Form Functionality
            function openForm() {
                document.getElementById('formContainer').style.display = 'block';
            }

            function closeForm() {
                document.getElementById('formContainer').style.display = 'none';
                document.getElementById('reviewForm').reset();
                document.querySelectorAll('.rating-stars label').forEach(l => l.style.color = '#ddd');
                document.getElementById('successMessage').style.display = 'none';
                document.getElementById('errorMessage').style.display = 'none';
            }

            // Star rating interactive functionality
            document.addEventListener('DOMContentLoaded', function () {
                const stars = document.querySelectorAll('.rating-stars input[type="radio"]');
                const starLabels = document.querySelectorAll('.rating-stars label');

                function updateStarColors(selectedIndex) {
                    starLabels.forEach((l, i) => {
                        l.style.color = (i <= selectedIndex) ? '#FFB800' : '#ddd';
                    });
                }

                starLabels.forEach((label, index) => {
                    label.addEventListener('click', function () {
                        updateStarColors(index);
                    });

                    label.addEventListener('mouseover', function () {
                        updateStarColors(index);
                    });
                });

                // Reset stars on mouse leave
                const ratingContainer = document.querySelector('.rating-stars');
                if (ratingContainer) {
                    ratingContainer.addEventListener('mouseleave', function () {
                        const checkedStar = document.querySelector('.rating-stars input[type="radio"]:checked');
                        if (checkedStar) {
                            const checkedIndex = Array.from(stars).indexOf(checkedStar);
                            updateStarColors(checkedIndex);
                        } else {
                            starLabels.forEach(l => l.style.color = '#ddd');
                        }
                    });
                }

                // Enhanced Form Submission Error Handling
                const reviewForm = document.getElementById('reviewForm');
                if (reviewForm) {
                    reviewForm.addEventListener('submit', function (e) {
                        e.preventDefault();

                        let ratingValue = 0;
                        const checkedRating = reviewForm.querySelector('input[name="rating"]:checked');
                        if (checkedRating) {
                            ratingValue = parseInt(checkedRating.value, 10);
                        }

                        const formData = {
                            name: reviewForm.name.value,
                            email: reviewForm.email.value,
                            // Important: send as "stars" and "content" to match backend model requirement
                            stars: ratingValue,
                            content: reviewForm.review.value,
                            consentToShare: reviewForm.consent.checked
                        };

                        document.getElementById('successMessage').style.display = 'none';
                        const errorDiv = document.getElementById('errorMessage');
                        errorDiv.style.display = 'none';
                        errorDiv.innerText = '';

                        fetch('/testimonials', {
                            method: 'POST',
                            headers: {
                                'Content-Type': 'application/json'
                            },
                            body: JSON.stringify(formData)
                        })
                            .then(async res => {
                                if(res.ok) {
                                    document.getElementById('successMessage').style.display = 'block';
                                    setTimeout(() => { closeForm(); window.location.reload(); }, 1500);
                                } else {
                                    // Enhanced: show validation errors for "stars" or "content", if present
                                    let d;
                                    try {
                                        d = await res.json();
                                    } catch (e) {
                                        d = { message: 'Submission failed. Please check your input.' };
                                    }

                                    let errorMsg = '';
                                    if (d && d.error && d.error.errors) {
                                        const errs = d.error.errors;
                                        if (errs.stars && errs.content) {
                                            errorMsg = 'Please provide a star rating and your review (both are required).';
                                        } else if (errs.stars) {
                                            errorMsg = 'Please provide a star rating.';
                                        } else if (errs.content) {
                                            errorMsg = 'Please write a review.';
                                        }
                                    }
                                    if (!errorMsg && d && d.message) {
                                        errorMsg = d.message;
                                    }
                                    if (!errorMsg) {
                                        errorMsg = 'Submission failed. Please check your input.';
                                    }

                                    errorDiv.innerText = errorMsg;
                                    errorDiv.style.display = 'block';
                                }
                            })
                            .catch(() => {
                                errorDiv.innerText = "Something went wrong. Please try again.";
                                errorDiv.style.display = 'block';
                            });
                    });
                }
            });
        </script>


